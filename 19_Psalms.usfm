\q1
\v 86 Nawq-euq zanrdawq khofdawq dawqtawvluf zif duidui-eu mae.
\q1 Ngaq-anr cawba naeqlaf-awq.
\q1 Aqyawfmaf, maqtsaq maqdawr-eu lafkhoer,
\q1 Ngaq-anr khaeqluf khavluf-eu mae.
\q1
\v 87 Ngar, mirtsaq-euq khoertav maqya daefni,
\q1 Aqyawfmaf nae bi lavbyov-euq law mir-a.
\q1 Aerkhawf, ngar, nawq-euq maeqdawq-anr maq byaerdzae-aq.
\q1
\v 88 Ngar, nawq-euq khaqmae-anr nae dovle-euq dawqdzawr-anr mrcaw nya’ni,
\q1 Nawq-euq maqtsaev-ir ni gaq-eu jeq-euq lafkhoer,
\q1 Daef-eu jeq bifnaeq laf-awq.
\b
\s1 Yaerdawq-anr Maqngaer
\qa ל LAQMER
\q1
\v 89 Ya’we Miqyaer-or!
\q1 Nawq-euq dawq-aq, mqmaq maqbyav yarzir mq-euq khoertav lavzif taq-euq mae.
\q1
\v 90 Nawr sarjax zif-eu jeq tiqzir sair tiqzir zif-awr,
\q1 Nawq nae ciqtsaq taq-euq mirtsaq tanqkhawr maqpov maqparni jav-eu mae.
\q1
\v 91 Nawq nae kadar naeq-euq baqda’nae, iqnan keuvni zifluf-eu mae.
\q1 Aqjeq mirnae aernar-aq,
\q1 Yawqjeq navluf nawq-euq kuirlaf zaqkhav pyeuf-eu mae.
\q1
\v 92 Ngar, nawq-euq zanrdawq-anr laqkhaq maqcaer-euq lawnmr-aq,
\q1 Ngar shaqzanq daqluf ngae yacawv-ir-eu mae.
\q1
\v 93 Nawr, nawq-euq maeqdawq-anr ghoe-awr nae,
\q1 Ngaq-anr daef-eu jeq bifnaeq laf-eu mirnae;
\q1 Ngar, nawq-euq dawq-anr tiqjawr-iq maqngaer.
\q1
\v 94 Ngar-aq nawq-euq ma; Ngaq-anr kakhof naeqlaf-awq.
\q1 Aqjeq mirnae aernar-aq, ngar, nawq-euq maeqdawq-anr sharpov ma.
\q1
\v 95 Tsawrcawv deuq, ngaq-anr lavcawv-eu laer gayuf-awr lofluf-eu khawf,
\q1 Ngar, nawq-euq dawqdzawr-anr sharnoeq sharsawx ma.
\q1
\v 96 Ngaq nae ya’mawr-euq-aq,
\q1 Yawqjeq navluf byan-awr kuf-eu khoer-aq, yawdui baw-eu mae.
\q1 Aerkhawf, nawq-euq zanrdawq khofdawq-aq, jerloni ghae-eu mae.
\b
\s1 Yaerdawq-euq Baqda’nae Dzaeq-eu
\qa מ MEM
\q1
\v 97 Or-oq, ngar, nawq-euq zanrdawq-anr arnavni gaq-eu nga!
\q1 Nan’ga-i ngar, nawq-euq zanrdawq-anr dzardziv dziv-eu ma.
\q1
\v 98 Ngaq naeq-aer aqmyanluf, nawq-euq zanrdawq khofdawq jav-eu mirnae,
\q1 Ngar, ngaq-anr muir-eu deuq-euq navmaqtser ganrdzaeq ma.
\q1
\v 99 Ngar, nawq-euq dawqdzawr deuq dzardziv dziv-eu mirnae,
\q1 Ngar, ngaq-anr maeq-euq deuq dawqtawvluf navmaqtser ghovshawnya dzaeq-eu ma.
\q1
\v 100 Ngar, nawq-euq maeqdawq-anr mrcaw-eu mirnae,
\q1 Ngar, tsawrmawq deuq-euq navmaqtser ghovshawnya dzaeq-eu ma.
\q1
\v 101 Ngar, nawq-euq dawq-anr mrcaw mawf-eu mirnae,
\q1 Yawdoer garma yawqkanr navluf-anr nae,
\q1 Ngaq-euq aqkuir cawqkhof-eu ma.
\q1
\v 102 Nawr, ngaq-anr maeqnaeq laf-euq mirnae,
\q1 Ngar, nawq nae yarkhaq dar-euq jeq-anr maq byaerdzae-aq.
\q1
\v 103 Nawq-euq dawq-anr ngar myeufhaw ngae, arnavni coer-eu nya!
\q1 Ngaq-euq khaqmae-euq gharduq, byaquir navmaqtser coer nya!
\q1
\v 104 Ngar, nawq-euq maeqdawq-euq baqda’nae, ghovshawnya lar-euq mirnae,
\q1 Ngar, maqtsaq maqdawr-eu garma yawqkanr navluf-anr byae nya.
\b
\s1 Yaerdawq-euq Yawshaef
\qa נ NUQ
\q1
\v 105 Nawq-euq dawq-aq, ngaq-euq aqkuir-euq gharduq tsirkhmq pyeuf-awr,
\q1 Ngaq-euq garma-anr yawshaef pyeuf-eu mae.
\q1
\v 106 Ngar, nawq-euq tsaq-awr dawr-euq zanrsanrkhof mrcaw ma laer,
\q1 Kamq-awr yawduq lavbaw-euq ma.
\q1
\v 107 Ya’we Miqyaer-or!
\q1 Ngar shirtaw taeqni ya daqluf-eu gar;
\q1 Nawq-euq dawq aertaq-euq lof-aer,
\q1 Ngaq-anr lavdaef naeqlaf-awq.
\q1
\v 108 Ya’we Miqyaer-or!
\q1 Ngar, nawq-anr nuima keuv-eu lafkhoer ngaq-euq khaqmae nae,
\q1 Jaceuq-eu jeq lawrnaeq-eu gar hageuf naeqlaf-awr,
\q1 Nawq-euq yarkhaq dar-eu jeq, ngaq-anr maeqnaeq laf-awq.
\q1
\v 109 Aqmyanluf, ngar, ngaq-euq daefzir,
\q1 Ngaq-euq aqlaf-euq lafkhoer yurluf-eu ma.
\q1 Aerkhawf, ngar, nawq-euq zanrdawq-anr maqngaer.
\q1
\v 110 Tsawrcawv, ngaq-anr lanrju khmrtaq-euq mae.
\q1 Aerkhawf, ngar, nawq-euq maeqdawq-anr nae maq canbyov.
\q1
\v 111 Nawq-euq dawqdzawr-aq, ngaq-euq nuima gharduq laqkhaq caer-eu jeq pyeuf-eu mirnae,
\q1 Mqmaq maqbyav yarzir ngaq-euq jeqbir pyeuf-eu mae.
\q1
\v 112 Ngar, mqmaq maqbyav yarzir arsaqsaq ngae keuvni,
\q1 Ngaq-euq nuima, nawq-euq zanrtaq khoftaq-anr hageuf-awr mrcaw ma.
\b
\s1 Yaerdawq-anr Mrcaw Ma
\qa ס SAQMEV
\q1
\v 113 Ngar, nuima nyiq siq tsof-awr povpov lavlav-aer ghae-eu tsawrhaq-anr byae nya.
\q1 Aerkhawf, ngar, nawq-euq zanrdawq-anr gaq-eu ma.
\q1
\v 114 Nawr-aq, ngar caevzafduq hawr-eu ngaq-euq daeqdaqduq pyeuf-awr,
\q1 Ngar, nawq-euq dawq-anr noeqtmq ma.
\q1
\v 115 Yawdoer jeq mr-eu tsawrhaq deuq-or!
\q1 Caev-awq, ngaq-euq dawqpae taqjawr.
\q1 Aqjeq mirnae aernar-aq,
\q1 Ngar, ngaq-euq Miqyaer-euq zanrdawq khofdawq-anr mrcaw ma!
\q1
\v 116 Ngar yawdaef yadaefni, nawq-euq dawq aertaq-euq lof-aer,
\q1 Ngaq-anr nyaefcuf naeqlaf.
\q1 Ngaq nae noeqtmq-euq jeq-euq lafkhoer daq taqbi ga naeqlaf.
\q1
\v 117 Ngar yabuq-ir ni, ngaq-anr ciqtur naeqlaf-awq;
\q1 Khoernar-aq, ngar, nawq-euq zanrtaq khoftaq-anr aqmyanluf cantae ma.
\q1
\v 118 Nawr, nawq-euq zanrtaq khoftaq-anr nae garbanq banq-ir-euq deuq-anr byaerdzae-eu mae.
\q1 Aqjeq mirnae aernar-aq, aqyawf maf-euq maershuir-aq, jaqmyawf pyeuf-eu mae.
\q1
\v 119 Nawr, mirtsaq-euq tsawrcawv dawqtawv luf-anr,
\q1 Javguv javmeuq lof-aer shadzae-eu mirnae,
\q1 Ngar, nawq-euq dawqdzawr-anr gaq-eu ma.
\q1
\v 120 Ngar, nawq-anr guv-eu mirnae, ngaq-euq shiqshaq yawdzuqdzuq mir-a.
\q1 Nawr yarkhaq dar-eu gar ngar yawkhav-aer guv nya.
\b
\s1 Yaerdawq Maeqnaeqlaf
\qa ע AQYINAV
\q1
\v 121 Ngar, dawr-awr tsaq-eu jeq hawr-eu tsaq-awr dawr-eu jeq mr-euq ma.
\q1 Ngaq-anr khaeqlar-eu deuq-euq lafkhoer taq byaerdzaeyaf naeqlaf.
\q1
\v 122 Nawq-euq kuirlaf zaqkhav yawmuiq jeq za’ni yawduq lavbaw naeqlaf-awq;
\q1 Nuima huiq-eu tsawrhaq deuq nae taqbi khaeqnaeqlaf.
\q1
\v 123 Nawq-euq kakhof-eu jeq hawr-eu nawq-euq tsaq-awr dawr-eu dawq-anr,
\q1 Yawkhav-aer povtser-euq mirnae, ngaq-euq myavnuiv, myavghaq baq mir-a.
\q1
\v 124 Nawq-euq kuirlaf zaqkhav-anr,
\q1 Nawq-euq ghawqshaq gaq-eu jeq-anr geuxni janrnaeq laf-awr,
\q1 Nawq-euq zanrtaq khoftaq maeqnaeq laf-awq.
\q1
\v 125 Ngar-aq, nawq-euq kuirlaf zaqkhav ma;
\q1 Ngar, nawq-euq dawqdzawr-anr siqnya larni,
\q1 Ngaq-anr ghovshaw nya-eu jeq bifnaeq laf-awq.
\q1
\v 126 Ya’we Miqyaer-or!
\q1 Tsawrhaqzaq, nawq-euq zanrdawq-anr yawqngaef laer sawxtaq-euq mirnae,
\q1 Nawr tiqjeqpaq janr-eu aryamq tsaqlar-euq mae.
\q1
\v 127 Ngar, shuir-anr navmaqtser, muiq-eu shuir-anr navmaqtser,
\q1 Nawq-euq zanrdawq khofdawq-anr gaq-eu ma.
\q1
\v 128 Khoermirnae, ngar, yawqjeq navluf-anr taf-eu nawq-euq maeqdawq dawqtawvluf tsaq-eu ngar-a laer noeqduq-awr,
\q1 Ngar, jaqmyawf-euq garma yawqkanr navluf-anr byae-eu ma.
\b
\s1 Yawshaef Bif-eu Yaerdawq
\qa פ PE
\q1
\v 129 Nawq-euq dawqdzawr-aq, zanqler paq-aer muiq-eu mirnae,
\q1 Ngaq-euq nuima saflar cantae-eu ma.
\q1
\v 130 Nawq-euq dawq urlar ngae yawshaef bif-awr,
\q1 Maqdzaw-euq zaq-anr ghovshaw-eu jeq bif-eu mae.
\q1
\v 131 Ngar, ngaq-euq khaqmae mrnga-awr safma ghawrluf-eu ma;
\q1 Aqjeq mirnae aernar-aq,
\q1 Ngar, nawq-euq zanrdawq khofdawq-anr yawkhav-aer za’mawf nya.
\q1
\v 132 Nawr, nawq-euq tsawrmyanr-anr gaq-eu tsawrhaq deuq-anr,
\q1 Ngaeqghae naeq-eu zanrsanrkhof jav-eu lof-aer,
\q1 Ngaq-anr haw-awr, ngaq-anr ghawqshaq gaqnaeq laf-awq.
\q1
\v 133 Ngaq-euq kuirtof-anr, nawq-euq dawq nae lavzif naeqlaf-awr,
\q1 Yawlaeq yawca jeq aqgeu jeq nae maqkaq ngaq-anr taqbi ghaqlar naeqlaf.
\q1
\v 134 Ngar, nawq-euq maeqdawq-anr mrcaw nya’ni,
\q1 Ngaq-anr khaeqlar-eu tsawrhaq-anr nae kakhof naeqlaf-awq.
\q1
\v 135 Nawq-euq kuirlaf zaqkhav-euq khoertav,
\q1 Nawq-euq myavpyawq-euq yawshaef bya’naeq lef-awr,
\q1 Nawq-euq zanrtaq khoftaq, ngaq-anr maeqnaeq laf-awq.
\q1
\v 136 Tsawrhaqzaq, nawq-euq zanrdawq-anr maq daefcaw-eu mirnae,
\q1 Ngaq-euq myavnuiv-anr nae myavbir lawrgawv yoerluf-eu mae.
\b
\s1 Sarjax Zif-eu Yaerdawq
\qa צ SADE
\q1
\v 137 Ya’we Miqyaer-or!
\q1 Nawr-aq, tsaq-awr dawr-eu mirnae,
\q1 Nawr yarkhaqdar ngae dawrcoeni dar-eu mae.
\q1
\v 138 Nawq nae bif-euq dawq, nawq-euq dawqdzawr-aq,
\q1 Tsaq-awr dawr-eu uqtaf, sarjax zif duidui-eu mae.
\q1
\v 139 Ngaq-anr muir-eu deuq, nawq-euq dawq-anr ngaer-eu mirnae,
\q1 Ngar nuima keuv-eu jeq, miqdzaq dofluf-eu lof-aer javluf mir-a.
\q1
\v 140 Nawq-euq dawq-aq, shawr duidui-eu mae.
\q1 Khoermirnae, nawq-euq kuirlaf zaqkhav, nawq-euq dawq-anr gaq-eu ma.
\q1
\v 141 Aqcawq, ngaq-anr taqnyir-awr hawdoer-eu khawf,
\q1 Ngar, nawq-euq maeqdawq-anr maqngaer.
\q1
\v 142 Nawq-euq tsaq-awr dawr-eu jeq-aq,
\q1 Mqmaq maqbyav yarzir gmq-eu, tsaq-awr dawr-eu jeq pyeuf-awr;
\q1 Nawq-euq zanrdawq-aq, yawtsaq yawdawr mae.
\q1
\v 143 Ngar, gardzaq garkawv hawr-eu nuima cawv duidui-eu jeq nae bi taemi-euq khawf,
\q1 Nawq-euq zanrdawq khofdawq-aq,
\q1 Ngaq-euq laqkhaq caerduq pyeuf-eu mae.
\q1
\v 144 Nawq-euq dawqdzawr-euq tsaq-awr dawr-eu jeq-aq,
\q1 Mqmaq maqbyav yarzir zif-eu mae;
\q1 Ngar yawdaef yadaefni, ngaq-anr ghovshaw nya-eu jeq bifnaeq laf-awq.
\b
\s1 Yaerdawq Dzardziv Dziv-eu
\qa ק KOR
\q1
\v 145 Ya’we Miqyaer-or!
\q1 Ngaq-anr narhaq naeqlaf-awq!
\q1 Ngaq-euq nuima dawqtawvluf,
\q1 Nawq-anr gur-awr sharluf-eu ma.
\q1 Ngar, nawq-euq zanrtaq khoftaq-anr mrcaw ma.
\q1
\v 146 Ngar, nawq-anr gur-awr sharluf-eu ma;
\q1 Ngaq-anr kakhof naeqlaf-awq,
\q1 Ngar, nawq-euq dawqdzawr-anr mrcaw ma.
\q1
\v 147 Ngar, uqshawq paw nanrma maq dovlar-euq minav-aer tur-awr,
\q1 Ngaq-anr cawba naeqlaf laer gurluf-eu ma;
\q1 Ngar, nawq-euq dawq-anr noeqtmq ma.
\q1
\v 148 Ngar, nawq-euq dawq-anr dzardziv dziv-eu mirnae,
\q1 Uqcif mirlan-aer ngaq-euq myavnuiv byaeluf-eu ma.
\q1
\v 149 Ya’we Miqyaer-or!
\q1 Nawr ghawqshaq gaqnya-eu gar tsaqni,
\q1 Ngaq-euq tersar-anr narhaq naeqlaf-awq;
\q1 Nawr, dawr-awr tsaq-eu gar geuxni, ngaq-anr lavdaef naeqlaf-awq.
\q1
\v 150 Nawq-euq zanrdawq-anr nae garmanr kaq-eu gar jawr-awr,
\q1 Yawdoer jeq-euq maeqnan cantae-eu tsawrhaq deuq,
\q1 Ngaq-euq dawqpae oeluf-eu mae.
\q1
\v 151 Ya’we Miqyaer-or!
\q1 Nawr dawqpae jawrluf-eu mae;
\q1 Nawq-euq zanrdawq khofdawq dawqtawvluf yawtsaq yawdawr mae.
\q1
\v 152 Nawq-euq dawqdzawr-euq garkawv, mqmaq maqbyav yarzir zifni,
\q1 Dawqpaeq lavzif taq-euq mae laer,
\q1 Ngar arhuba nae siqnya-euq ma.
\b
\s1 Yaerdawq-aq, Yawtsaq Yawdawr Mae
\qa ר LERSHAV
\q1
\v 153 Ngar, nawq-euq zanrdawq-anr maqngaer-eu mirnae,
\q1 Ngar shaqzanq ya daqluf-eu garkawv sharnoeq naeqlaf-awr,
\q1 Ngaq-anr kakhof naeqlaf-awq.
\q1
\v 154 Ngaq-anr yarkhaq jadan naeqlaf-awr,
\q1 Ngaq-anr kakhofdov naeqlaf-awq;
\q1 Nawq-euq dawq aertaq-euq lof-aer,
\q1 Ngaq-anr lavdaef naeqlaf-awq.
\q1
\v 155 Tsawrcawv deuq-aq, nawq-euq zanrtaq khoftaq-anr maqpov-eu mirnae,
\q1 Kakhof-eu jeq-anr nae garmanr kaq-eu gar jawr-eu mae.
\q1
\v 156 Ya’we Miqyaer-or!
\q1 Nawr nanqyuni ghawqshaq gaq-eu jeq huiq duidui mae;
\q1 Nawr yarkhaq dar-eu gar tsaqni, ngaq-anr lavdaef naeqlaf-awq.
\q1
\v 157 Ngaq-anr muir-eu deuq hawr-eu,
\q1 Ngaq-anr khaeq-awr khav-eu deuq, myaq duidui-eu khawf,
\q1 Ngar, nawq-euq dawqdzawr-anr nae yawqpawyawq maq ciqpyawr.
\q1
\v 158 Ngar, sarjax maqzif-eu deuq-anr, hawmawr-awr nuima dav duidui nya;
\q1 Aqjeq mirnae aernar-aq, aqyawfmaf, nawq-euq dawq-anr maqmr caw-a.
\q1
\v 159 Ngar, nawq-euq maeqdawq-anr arnavni gaq-eu nga laer sharnoeq naeqlaf-awr;
\q1 Ya’we Miqyaer-or!
\q1 Nawr ghawqshaq gaqnya-eu gar tsaqni, ngaq-anr lavdaef naeqlaf-awq.
\q1
\v 160 Dawqtawv-euq uqduq aer ngae,
\q1 Nawq-euq dawq-aq, yawtsaq yawdawr mae;
\q1 Nawq nae tsaq-awr dawr-eu lafkhoer yarkhaq khardar-eu jeq-aq, mqmaq maqbyav yarzir gmq-eu mae.
\b
\s1 Yaerdawq-anr Laqkhaq Caer-eu
\qa ש SHIQNAV
\q1
\v 161 Dzoeqda deuq, ngar tiqjeq-iq maqlaeq-eu gar, ngaq-anr khaeqlar-eu khawf,
\q1 Ngaq-euq nuima-aq, nawq-euq dawq-anr yawkhav-aer zanqler mir-a.
\q1
\v 162 Pyurpanq shuirpanq mawr-euq tsawrhaq tiq ghaq laqkhaq caer-eu lof-aer,
\q1 Ngar, nawq-euq dawq-anr laqkhaq caer-eu ma.
\q1
\v 163 Ngar, jaqmyawf myawf-eu jeq-anr nuidav dav-awr byae-eu ma.
\q1 Aerkhawf, ngar, nawq-euq zanrdawq-anr gaq-eu ma.
\q1
\v 164 Nawr yarkhaq dar-eu tsaq-awr dawr-eu mirnae,
\q1 Ngar, nawq-anr tiq nan shif pov jaceuq ma.
\q1
\v 165 Nawq-euq zanrdawq-anr gaq-eu deuq-aq,
\q1 Nuisar of-eu jeq huiq duidui-awr,
\q1 Tiqjeq-iq, aqyawf maf-anr sanqtif maqbi tifler nya-a.
\q1
\v 166 Ya’we Miqyaer-or!
\q1 Ngar, nawq-euq kakhof-eu jeq-anr noeqtmq-awr,
\q1 Ngar, nawq-euq zanrdawq khofdawq mrcaw luf-eu ma.
\q1
\v 167 Ngar, nawq-euq dawqdzawr-anr yawkhav-aer gaq-awr,
\q1 Ngaq-euq nuima saflar nae cantae luf-eu ma.
\q1
\v 168 Ngaq-euq garma dawqtawvluf, nawq-euq maeqshi jav-awr,
\q1 Ngar, nawq-euq maeqdawq hawr-eu dawqdzawr-anr mrcaw luf-eu ma.
\b
\s1 Yaerdawq-anr Tsergeuf-eu
\qa ת TAO
\q1
\v 169 Ya’we Miqyaer-or!
\q1 Ngar gur-awr shar-eu nawr jawr-eu gar bi larkeuv naeqlaf-awq.
\q1 Nawq-euq dawq aertaq-euq lof-aer,
\q1 Ngaq-anr ghovshaw nya-eu jeq bifnaeq laf-awq.
\q1
\v 170 Ngaq nae tanqshar-euq dawq,
\q1 Nawq-euq maeqshi bi larkeuv-awr;
\q1 Nawq-euq dawq aertaq-euq lof-aer, ngaq-anr bibuq-ir naeqlaf-awq.
\q1
\v 171 Nawr, ngaq-anr zanrtaq khoftaq maeqnaeq laf-euq mirnae,
\q1 Ngaq-euq maeqluq-anr nae jaceuq-eu dawqkawq bi yoerdovlef ma.
\q1
\v 172 Nawq-euq zanrdawq khofdawq dawqtawvluf,
\q1 Tsaq-awr dawr-eu mirnae, ngaq-euq maeqlar,
\q1 Nawq-euq dawq-euq garkawv ngaeq ma.
\q1
\v 173 Ngar, nawq-euq maeqdawq-anr tsergeuf-eu mirnae,
\q1 Nawq-euq aqlaf, ngaq-anr cawba naeqlaf-awq.
\q1
\v 174 Ya’we Miqyaer-or!
\q1 Ngar, nawq-euq kakhof-eu jeq-anr lofluf-eu ma;
\q1 Nawq-euq zanrdawq-aq, ngaq-euq laqkhaq caerduq pyeuf-eu mae.
\q1
\v 175 Ngaq-euq nuima saflar, nawq-anr jaceuq nya’ni,
\q1 Yawdaef bidaef naeqlaf-awq;
\q1 Nawq nae yarkhaq dar-euq jeq, ngaq-anr bi cawba naeqlaf-awq.
\q1
\v 176 Ngar, nawq-euq zanrdawq khofdawq-anr maqngaer-euq mirnae,
\q1 Byov-ir-euq yawrzaq lof-aer garma canbanq taq-euq,
\q1 Nawq-euq kuirlaf zaqkhav-anr povkhofle naeqlaf-awq.
\c 120
\s1 Muir-eu Deuq-anr Nae Bibuq-ir Naeqlaf
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Ngar guv-awr gav-eu jeq-anr tanqpuq ngae,
\q1 Ya’we Miqyaer-anr kur ma.
\q1 Aqyawf, ngaq-anr narhaq naeqlaf-eu mae.
\q1
\v 2 Ya’we Miqyaer-or!
\q1 Jaqmyawf myawf-eu maeqluq hawr-eu maershuir shuir-eu maeqlar-anr nae,
\q1 Ngaq-euq nuima saflar-anr bibuq-ir naeqlaf.
\b
\q1
\v 3 Jaqmyawf myawf-eu maeqlar-or!
\q1 Nawq-anr aqjeq bi keuvle-awr,
\q1 Nawq-anr aqjox-aer janrnaeq laf-eu jeq la laer,
\q1 Nawr siqnya ma laq?
\q1
\v 4 Tav duidui-eu guirzaq-euq kavmyaq hawr-eu,
\q1 Yavpyawr arbawr-euq khaqgheuq-anr\f + \fr 120:4 \fk Yavpyawr arbawr \ft Arbawr heujeq-aq, zawr muiq-eu khaqgheuq panqdov laf-eu kharshuif dae-anr daef-eu arbawr tiqjeq mae.\f* nerhoqni dofluf-eu miqdzaq miqdziv nae darnaeqlaf taef-a.
\b
\q1
\v 5 Ngar, Geqdaq tsawrhaq-euq saeqghawv-euq ghanrcae daef-awr,
\q1 Meqshaev mirkhanq-anr jawr-euq mirnae, ghawqshar shaq-aer ghaesaef mir-a.
\q1
\v 6 Ngaq-euq nuima saflar, tsaqler dzoerler-eu jeq-anr byae-eu tsawrhaq naeq-aer,
\q1 Ardzaeq-aer jawrmyanr dzeur-euq mir-a.
\q1
\v 7 Ngar-aq, tsaqler tsanrler-eu jeq-euq gharduq daef-eu ma.
\q1 Aerkhawf, ngar ja ngae aqyawfmaf guirdeuv deuv-eu garkawv ja-eu mae.
\c 121
\s1 Hawpir-eu Ya’we Miqyaer
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Ngar, ngaq-euq myavnuiv ciqpyawr-awr gawqjawq paw hawdav lef-eu ma;
\q1 Ngaq-anr cawba-eu jeq aqgeugar nae dovle-eu la?
\q1
\v 2 Ngaq-anr cawba-eu jeq-aq,
\q1 Mq hawr-eu mir-anr bae-euq Ya’we Miqyaer-anr nae dovle-eu mae.
\b
\q1
\v 3 Aqyawf, nawq-euq aqkuir-anr sanqtif maqbi tif-ar.
\q1 Nawq-anr hawpir-eu ghaq aqmyanluf yawnoeq mae.
\q1
\v 4 Hawsher-eq, Irsaqleqlav-anr hawpir-eu ghaq-aq,
\q1 Yufyoer maqngeur-aer yufkhav-eu maqbaw-a.
\b
\q1
\v 5 Ya’we Miqyaer-aq, nawq-anr hawpir-eu ghaq pyeuf-eu mae;
\q1 Ya’we Miqyaer-aq,
\q1 Nawq-euq armar paw, nawr narshmr awduq pyeuf-eu mae.
\q1
\v 6 Nan’ghanr paw, nanrma, nawq-anr maqcoeq maqciv-aer;
\q1 Uqcif paw, bala, nawq-anr maqcoeq maqciv-ar.
\b
\q1
\v 7 Yawdoer jeq dawqtawv luf-anr nae,
\q1 Ya’we Miqyaer, nawq-anr hawpir naeqlaf-awr;
\q1 Aqyawf, nawq-euq nuima saflar-anr hawpir naeqlaf taef-a.
\q1
\v 8 Nymq-mr nae mqmaq maqbyav yarzir keuvni,
\q1 Ya’we Miqyaer, nawr dov-ir-awr urlar-eu gar, hawpir naeqlaf taef-a.
\c 122
\s1 Yeqlursaqlaeq-euq Gharduq Guiqlanq Shar-awq
\d Ledav ngae car-eu Daqbir-euq dawqcar
\q1
\v 1 Aqcawq, ngaq-anr heulof-aer aer ngae, ngar laqkhaq caer-euq ma,
\q1 “Dzawq! Ya’we Miqyaer-euq nymr-anr le kar-eur.”
\q1
\v 2 Yeqlursaqlaeq-or! Nga’maf-euq aqkuir, nawq-euq lanrkhaeq-euq lafkhoer yawfluf-eu ma.
\b
\q1
\v 3 Yeqlursaqlaeq-aq, tiqgar taef-aer tsanrdzeufni tsovtaq-euq meuq tiq meuq lof-aer dur-eu mae.
\q1
\v 4 Khoergar, tsawrjeuq deuq uiqler-eu mae.
\q1 Ya’we Miqyaer-euq tsawrjeuq,
\q1 Irsaqleqlav tsawrhaq-euq Dawqdzawr Bawqkeuv\f + \fr 122:4 \fk Dawqdzawr Bawqkeuv \ft Heu-aq, dawqma sarjax bawqkeuv-anr aermawf-eu mae.\f* jav-eu gar le-awr,
\q1 Ya’we Miqyaer-euq tsawrmyanr-euq guiqlanq-anr jaceuq ler-eu mae.
\q1
\v 5 Aqjeq mirnae aernar-aq,
\q1 Yarkhaq dar-eu nuirmanq pyeuf-eu Daqbir-euq nymrkhanq-euq nuirmanq khoergar jav-eu mae.
\b
\q1
\v 6 Yeqlursaqlaeq nuisar ofni guiqlanq shar-awq:
\q1 “Nawq-anr gaq-eu tsawrhaq deuq muiqlar sanrlar luf-ur.
\q1
\v 7 Nawq-euq meuqlanr-euq lafkhoer tsaqler dzoerler-eu jeq jav-awr,
\q1 Sanqpaq-euq nymr-euq lafkhoer bawle zale luf-ur.”
\q1
\v 8 Ngar, ngaq-euq maernmr deuq hawr-eu cawqdeuq-euq gharduq heulof-aer aer-eu ma,
\q1 “Nawq-euq lafkhoer nuisar of-eu jeq jav luf-ur.”
\q1
\v 9 Aqdui-euq Miqyaer pyeuf-eu Ya’we Miqyaer-euq nymr-euq baqda’nae,
\q1 Ngar, nawq-euq gharduq yawmuiq jeq pov ma.
\c 123
\s1 Ghawqshaq Gaqnaeqlaf
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Mq-euq khoertav jawr-eu ghaq-or!
\q1 Ngaq-euq myavnuiv, nawr jawr-eu paw ciqpyawr laf-awr hawluf-eu ma.
\q1
\v 2 Hawsher-eq, kuirlaf zaqkhav tiq ghaq-euq myavnuiv,
\q1 Aqyawf-euq yawsanr-euq aqlaf-anr haw-eu lof-aer,
\q1 Zaqmiq aqbur zaqkhav tiq ghaq, aqyawf-euq nymrsanr aqma-euq aqlaf-anr haw-eu lof-aer durni;
\q1 Nga’maf-euq myavnuiv, nga’maf-euq Miqyaer pyeuf-eu Ya’we Miqyaer-anr hawluf-eu ma.
\q1 Aqyawf, ghawqshaq maq gaqnaeq laf-euq khoerkeuvni haw ma.
\b
\q1
\v 3 Ya’we Miqyaer-or! Nga’maf-anr ghawqshaq gaqnaeq laf-awq.
\q1 Nga’maf-anr ghawqshaq gaqnaeq laf-awq.
\q1 Aqjeq mirnae aernar-aq,
\q1 Nga’maf dawqdoer-anr ardzaeq-aer ya daqluf-eu nya.
\q1
\v 4 Nuima huiq-eu tsawrhaq hawteq hawdoer lar-awr,
\q1 Sarsuqni daef-eu deuq dawqdoer ngaeq-eu gar,
\q1 Nga’maf-euq nuima saflar maq daqcoerni, ardzaeq dzeurlar taq-euq nya.
\c 124
\s1 Aqdui-euq Paw Pyawr-eu Miqyaer
\d Ledav ngae car-eu Daqbir-euq dawqcar
\q1
\v 1 Irsaqleqlav tsawrhaq-or!
\q1 Heulof-aer aer-awq,
\q1 “Ya’we Miqyaer, aqdui jawr-eu paw maqpyawr-euq lawnmr-aq,”
\q1
\v 2 “Ya’we Miqyaer, aqdui jawr-eu paw maqpyawr-euq lawnmr-aq;
\q1 Aqdui-anr muir-eu tsawrhaq, turlar-awr caelar ngae,
\q1
\v 3 Aqyawfmaf, aqdui-anr miqdzaq dof-eu lof-aer nuima paer ngae,
\q1 Aqdui-anr yawdaef-aer myeufanr jir-euq ngeur-eu dur;
\q1
\v 4 Khoerngae, aqdui, ircuf nae bi danqpymq-awr,
\q1 Aqdui-euq nuima saflar lawrgawv-anr gapuf yaf-euq ngeur-eu dur;
\q1
\v 5 Khoernar-aq, aqdui-anr myeufanr-euq ircuf,
\q1 Aqdui-euq nuima saflar-anr danqpymqjir naeqlaf-euq ngeur-eu dur.”
\b
\q1
\v 6 Aqyawf maf-euq seuq nae, aqdui-anr maqbi kawfsaef dzaq-euq mirnae,
\q1 Ya’we Miqyaer-anr jaceuq ma.
\q1
\v 7 Aqdui-euq nuima saflar-aq,
\q1 Arjir tanr-eu saeqtov-anr nae dzeur-ir-euq arjir tiq mawr-anr dur-eu ma.
\q1 Saeqtov khaef-ir-euq mirnae, aqdui yadzeur-ir-euq ma.
\b
\q1
\v 8 Aqdui-anr cawba-eu jeq-aq,
\q1 Mq hawr-eu mir bae-euq Ya’we Miqyaer tsawrmyanr-euq lafkhoer jav-eu mae.
\c 125
\s1 Muiq-eu Deuq-anr Muiqnaeq-awq
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Ya’we Miqyaer-anr ghoe-eu tsawrhaq deuq-aq,
\q1 Tiqjawr-iq maq nyilar-aer, mqmaq maqbyav yarzir zif-eu Ziroq gawqjawq-anr dur-eu mae.
\q1
\v 2 Yeqlursaqlaeq meuq-anr gawqjawq jawqlanr taq-euq lof-aer,
\q1 Nymq-mr nae mqmaq maqbyav yarzir keuvni,
\q1 Ya’we Miqyaer, aqyawf-euq tsawrhaq deuq-euq tiqjawq jawrjawq taq-euq mae.
\b
\q1
\v 3 Tsaq-awr dawr-eu tsawrhaq deuq-euq mirkhanq-anr,
\q1 Tsawrcawv-euq shmr gawqyoeq maqya naq-ar.
\q1 Maqngeur nar-aq, tsaq-awr dawr-eu tsawrhaq deuq,
\q1 Aqyawf maf-euq aqlaf cawqdov-awr yawlaeq yawca jeq mr taef-a.
\b
\q1
\v 4 Ya’we Miqyaer-or!
\q1 Muiq-eu deuq-anr hawr-eu nuima dawrcoeni ghae-eu deuq-anr,
\q1 Yawmuiq jeq mrnaeq-awq.
\q1
\v 5 Aerkhawf, garma yawngof yawngav-aer can-eu deuq hawr-eu,
\q1 Yawlaeq yawca jeq mr-eu deuq-anr-aq,
\q1 Ya’we Miqyaer taedzaenaeq taef-a.
\q1 Irsaqleqlav-euq khoertav nuisar of luf-ur.
\c 126
\s1 Yufmav Mav-euq Gar Dur-eu
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Aqcawq nae bi nyaefbyov ghae-euq tsawrhaq,
\q1 Ya’we Miqyaer nae, Ziroq-anr bi sheuqkhof laf-euq mirnae, yufmav mav-euq gar dur nya.
\q1
\v 2 Khoermirnae, aqdui-euq maeqlar dawqcar car-awr,
\q1 Aqdui-euq khaqmae tiqbyanni, maq naqler-aer uir-euq ma.
\q1 Khoerngae, mirkhanq tsawrjeuq deuq aer-eu,
\q1 “Ya’we Miqyaer nae, aqyawf maf-euq gharduq huiq-eu jeq mr-euq ngar-a.”
\q1
\v 3 Ya’we Miqyaer, aqdui-euq gharduq zawr huiq-eu jeq mr-euq mae.
\q1 Aqdui laqkhaq arnavni caer paq-aer ghae-eu nga.
\b
\q1
\v 4 Ya’we Miqyaer-or!
\q1 Lafov paw yoer-eu lawrbaq lof-aer,
\q1 Bi nyaefbyov ghae-euq nga’maf-euq tsawrhaq sheuqkhof naeqlaf-awq.
\q1
\v 5 Myavbir yoeryoer-aer saeq-eu tsawrhaq-aq,
\q1 Laqkhaq caertavni yayaeq taef-a.
\q1
\v 6 Siqyoeq saeq-eu laer, ngoerngoer-aer caer siqyoeq pi-awr dov-ir-eu tsawrhaq-aq,
\q1 Yawduq bawni, caer caerdzanq pi-awr laqkhaq caertavni ya ghoflar taef-a.
\c 127
\s1 Miqyaer-euq Guiqlanq-euq Baqda’nae
\d Ledav ngae car-eu Sorlaqmoq-euq dawqcar
\q1
\v 1 Ya’we Miqyaer maqtsov-eu lawnmr-aq,
\q1 Nymr tsov-eu tsawrhaq nae mr-euq gar-aq, yawqngaef mae.
\q1 Ya’we Miqyaer maq lofpir-eu lawnmr-aq,
\q1 Meuq-anr lofpir-eu tsawrhaq maq yufkhavni lof-eu khawf yawqngaef mae.
\q1
\v 2 Uqshawq yawnaf-aer tur-awr uqcif mirlanni,
\q1 Yawdaef daef-eu gharduq mrsaef lef-euq khawf,
\q1 Tiqjeq-iq gardanr maqtsoeq-a.
\q1 Aqjeq mirnae aernar-aq,
\q1 Miqyaer, aqyawf gaq-eu tsawrhaq-anr,
\q1 Yawmuiq-aer bi yufkhav-eu mae.
\b
\q1
\v 3 Hawsher-eq, zaqdeuq-aq,
\q1 Ya’we Miqyaer nae bif-euq jeqbir pyeuf-awr,
\q1 Zaq-euq jawrzar-euq arsiq-aq, mrpoeq lafsawq mae.
\q1
\v 4 Zaqguq ngae nae daefle-euq zaqdeuq-aq,
\q1 Guirzaq-euq aqlaf-euq lafkhoer anrluf-eu kavmyaq-anr dur-eu mae.
\q1
\v 5 Kavmyaq-euq banq-anr khoerlof-aer ghae-eu kavmyaq byan-eu tsawrhaq-aq, laqkhaq yacaer taef-a.
\q1 Aqyawfmaf, lanrkhaeq-anr muir-eu deuq naeq-aer ya jadzaq khawf,
\q1 Daqga-eu gar maqya keuv-ar.
\c 128
\s1 Miqyaer-anr Guv-eu Guiqlanq
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Ya’we Miqyaer-anr guv-awr,
\q1 Aqyawf-euq garma-anr can-eu yawqghaq navluf,
\q1 Guiqlanq huiq-eu mae.
\q1
\v 2 Nawr, yawqha-euq aqlaf nae mr-euq lof-aer yadzaq-awr, laqkhaq yacaer-eu uqtaf,
\q1 Kharngeur-eu jeq nawq-euq gharduq muiq taef-a.
\b
\q1
\v 3 Nawq-euq nymr-euq lafkhoer,
\q1 Nawq-euq miqzaq-aq, arsiq sanr-eu aqgaf bawduv-anr dur-awr;
\q1 Nawq-euq zaqdeuq-aq, nawq-euq hawqje tiqjawq daef-eu saqloq arbawr kanqzaq-anr dur taef-a.
\q1
\v 4 Khoermirnae, hawde-eq,
\q1 Ya’we Miqyaer-anr guv-eu tsawrhaq-aq,
\q1 Guiqlanq khoerlof-aer za taef-a.
\b
\q1
\v 5 Ziroq-anr nae Ya’we Miqyaer, nawq-anr guiqlanq bif luf-ur.
\q1 Nawr yawdaef khardaef-eu tiqzir,
\q1 Yeqlursaqlaeq-euq yawmuiq jeq ya’mawr luf-ur.
\q1
\v 6 Nawr, nawq-euq oeqpaq oeqma deuq-anr ya’mawr ngae keuvni, zirbaw luf-ur.
\q1 Irsaqleqlav-euq khoertav nuisar of luf-ur.
\c 129
\s1 Ziroq-anr Byae-eu Deuq Yacawv-ir-eu
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Irsaqleqlav-anr heulof-aer bi’aer-awq,
\q1 “Ngar, zaqguq ngae nae, aqyawfmaf, ngaq-anr khaeqnaeq khavnaeq laf-euq mae.”
\q1
\v 2 “Ngar, zaqguq ngae nae, aqyawfmaf, ngaq-anr khaeqnaeq khavnaeq laf-euq mae;
\q1 Aerkhawf, aqyawfmaf, ngaq-anr maq ghaqlar nya-a.
\q1
\v 3 Aqyawfmaf, ngaq-euq narkhan daema tsaeq-eu lof-aer,
\q1 Tsaeq-euq markhaeq ghawqmanrni javluf-eu mae.”
\q1
\v 4 Ya’we Miqyaer-aq, tsaq-awr dawr-eu mae;
\q1 Aqyawf, tsawrcawv-euq arcav deuvtsaevdzae jir naeq-euq mae.
\b
\q1
\v 5 Ziroq-anr kharbyae-eu tsawrhaq deuq, bi taekhof ghae-awr daq biga-ir luf-ur.
\q1
\v 6 Aqyawfmaf nymrmq-euq khoertav daef-eu yarmof lof-aer,
\q1 Maq huiqlar-euq minav-aer nyoeq-ir jir luf-ur.
\q1
\v 7 Caer yaeq-eu tsawrhaq, aqlaf byanlar paq-aer maqyur-eu uqtaf,
\q1 Caerdzanq baf-eu tsawrhaq khawf, aqyawf-euq lafdu nae maqbaf-ar.
\q1
\v 8 Aqyawfmaf jawr-eu gar khar cantsaev-eu tsawrhaq heulof-aer taqaer,
\q1 “Ya’we Miqyaer-euq guiqlanq, nawmaf-euq khoertav keuvle luf-ur.
\q1 Nga’maf, Ya’we Miqyaer-euq tsawrmyanr-euq uqmov,
\q1 Nawq-anr dawq ngaeqghae naeq-eu ma.”
\c 130
\s1 Ya’we Miqyaer-anr Dawq-eu
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Ya’we Miqyaer-or!
\q1 Naf duidui-eu gar nae, ngar, nawq-anr gurgaq luf-eu ma.
\q1
\v 2 Sanqpaq Miqyaer-or!
\q1 Ngaq-euq tersar-anr narhaq naeqlaf-awq!
\q1 Ghawqshaq gaqnaeqlaf laer,
\q1 Ngar tanqshar-eu tersar-anr nawq-euq naqbawr ciqkhmr naeqlaf-awq.
\b
\q1
\v 3 Ya’we Miqyaer-or!
\q1 Nawr yawbanq yawlaeq guitaq nar-aq,
\q1 Sanqpaq Miqyaer-or!
\q1 Aqsur ghaq yawf-awr jawrnya-eu?
\q1
\v 4 Aerkhawf, nawr jawr-eu gar iqbax pyaeq-eu jeq jav-eu mae;
\q1 Khoermirnae, aqcawq, nawq-anr guv-eu tsaq mae.
\b
\q1
\v 5 Ngar, Ya’we Miqyaer-anr dawq-eu ma;
\q1 Ngaq-euq nuima saflar dawq-awr,
\q1 Aqyawf-euq dawq-anr noeqtmq-eu ma.
\q1
\v 6 Uqcif paw lofpir-eu deuq uqbya byalar-eu gar dawq-eu navmaqtser,
\q1 Ngaq-euq nuima saflar, Sanqpaq Miqyaer-anr dawq-eu ma.
\q1 Uqcif paw lofpir-eu deuq uqbya byalar-eu gar dawq-eu navmaqtser dawq-eu ma.
\b
\q1
\v 7 Irsaqleqlav-or! Ya’we Miqyaer-anr noeqtmq-awq!
\q1 Aqjeq mirnae aernar-aq,
\q1 Ya’we Miqyaer jawr-eu gar maqtsaev-ir nya-eu gaq-eu jeq jav-awr,
\q1 Aqyawf naeq-aer tiqbyanni kakhof-eu jeq jav-eu mae.
\q1
\v 8 Aqyawf, Irsaqleqlav-anr kakhofnaeq taef-a;
\q1 Aqyawf-euq yawbanq yawlaeq dawqtawv luf-anr nae kakhofnaeq taef-a.
\c 131
\s1 Miqyaer-anr Noeqtmq-awq
\d Ledav ngae car-eu Daqbir-euq dawqcar
\q1
\v 1 Ya’we Miqyaer-or!
\q1 Ngaq-euq nuima maqhuiq-aer,
\q1 Ngaq-euq myavnuiv khawf, janrghae-eu jeq maqanr;
\q1 Ngar, ardzaeq-aer huiq-eu garkawv maq sharnoeq-aer,
\q1 Yawqha-anr maqgeux-eu jeq maq sharsawx-aq.
\q1
\v 2 Aerkhawf, ngar, aqcoer ghaev-euq zaq,
\q1 Aqyawf-euq aqma naeq-aer jawr-eu lof-aer,
\q1 Ngaq-euq nuima saflar ciqcurni hawr-eu cofngaerni jawrluf-eu ma;
\q1 Ngaq-euq lafkhoer urluf-eu ngaq-euq nuima saflar-aq,
\q1 Aqcoer ghaev-euq zaq-anr dur-eu mae.
\b
\q1
\v 3 Irsaqleqlav-or! Nymq-mr nae bae-awr mqmaq maqbyav yarzir,
\q1 Ya’we Miqyaer-anr noeqtmq-awq.
\c 132
\s1 Ya’we Miqyaer-euq Jawrduq Pyeuf-eu Ziroq
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Ya’we Miqyaer-or!
\q1 Daqbir-anr noeqmawr naeqlaf-awr,
\q1 Aqyawf nae yadaq-euq shaqzanq dawqtawv luf-anr noeqkeuv naeqlaf-awq.
\q1
\v 2 Aqyawf, Ya’we Miqyaer-anr kamq-awr,
\q1 Yaqkov-euq Baoq huiq-eu ghaq-anr sarjax dzaq-euq lafkhoer aer-eu;
\q1
\v 3 “Ngar marmarni ngaq-euq nymr-anr maq ghofler.
\q1 Sarsuqni yufnya-eu, ngaq-euq yufzar-anr khawf maq yufler.
\q1
\v 4 Ngar, ngaq-euq myavnuiv-anr maqbi yufkhav-aer,
\q1 Ngaq-euq myavnuiv-euq myavkhov khawf, yufyoer maqbi ngeur.
\q1
\v 5 Ngar, Ya’we Miqyaer-euq gharduq jawrzar maq povmawr-aer,
\q1 Yaqkov-euq Baoq huiq-eu ghaq, jawrduq maqjav-eu khoerkeuvni,
\q1 Ngar ghaqnaq maqnaq.”
\b
\q1
\v 6 Hawsher-eq, nga’maf heu-euq garkawv Erpaqlaqta-anr yagaq-awr,
\q1 Arbawr yar-euq daega-anr nga’maf ya’mawr-euq ma.
\q1
\v 7 Aqdui, Ya’we Miqyaer-euq saqpar saeqghawv-anr urler kar-eur;
\q1 Aqyawf-euq aqkuir cawqtaqduq-anr uqduq tanqler kar-eur.
\b
\q1
\v 8 Ya’we Miqyaer-or!
\q1 Tur-awr nae nawq-euq baoq-euq ♦dawqma sarjax bawqkeuv naeq-aer,
\q1 Nawq-euq ghaqnaq naqduq-anr jawrle-awq.
\q1
\v 9 Nawq-euq pirma deuq tsaq-awr dawr-eu maeqtsoeq bidm-awr,
\q1 Nawq-euq tsawrshawr deuq laqkhaq caer-awr bigur law.
\q1
\v 10 Nawq-euq kuirlaf zaqkhav Daqbir-anr noeqkeuv-awr,
\q1 Nawq nae tsaqtsov-euq ghaq-euq myavpyawq, yawqpawyawq taqbi pyawr-ir naeqlaf.
\b
\q1
\v 11 Ya’we Miqyaer, Daqbir-anr yawtsaq yawdawr-euq lafkhoer kamqtaq-euq mae;
\q1 Aqyawf kamqtaq-euq heu-anr nae maqpov maq parlar-a.
\q1 Aqyawf aer-eu, “Ngar, nawq-euq nuirmanq-anr, nawq-euq mawrdo-euq arsiq binuir ma.
\q1
\v 12 Nawq-euq zaqyo deuq, ngaq-euq dawqma sarjax-anr mrcaw-awr,
\q1 Ngar maeq-eu ngaq-euq dawqdzawr-anr mrcaw nar-aq,
\q1 Aqyawf maf-euq zaqyo deuq khawf, mqmaq maqbyav yarzir keuvni,
\q1 Nawq-euq nuirmanq-anr ya’nuir taef-a.”
\b
\q1
\v 13 Aqjeq mirnae aernar-aq,
\q1 Ya’we Miqyaer nae Ziroq-anr tserdov-awr,
\q1 Aqyawf-euq jawrduq jawrzar bi pyeuflar mawf-eu mae.
\q1
\v 14 Miqyaer aer-eu,
\q1 “Heugar-aq, mqmaq maqbyav yarzir, ngaq-euq ghaqnaq naqduq ma.
\q1 Ngar heugar jawrmawf-eu mirnae, ngar heugar jawr ma.
\q1
\v 15 Ngar, dawq ngaeqghae naeq-awr,
\q1 Ziroq kharmawf-eu jeq tiqbyan tiqlof biza-awr;
\q1 Irshaqzaq-anr hawqmaef tiqlof bi dzaqdaev ma.
\q1
\v 16 Ngar, Ziroq-euq pirma deuq-anr kakhof-eu maeqtsoeq bidm-awr,
\q1 Tsawrshawr deuq laqkhaq caer-eu tersar yawkhav-aer yagur taef-a.
\q1
\v 17 Ngar, heugar Daqbir-euq uqcoer tiq coer bi huiqlar-awr,
\q1 Ngaq nae tsaqtsov taq-euq ghaq-euq gharduq, tsirkhmq tiq khmq laeqhmq taq-euq ma.
\q1
\v 18 Ngar, aqyawf-anr muir-eu deuq-anr daqga-eu maeqtsoeq bidm ma.
\q1 Aerkhawf, aqyawf-euq uqduq-anr khovluf-eu shuir-euq uqkhov-aq, yawshaef bya taef-a.”
\c 133
\s1 Nuima Gm-eu Guiqlanq
\d Ledav ngae car-eu Daqbir-euq dawqcar
\q1
\v 1 Hawsher-eq, maernmr deuq nuima gm-awr tiqgar taef-aer jawrtaf-eu,
\q1 Arnavni muiq-awr arnavni nuikhanr shirlar paq-aer ghae-eu nga!
\q1
\v 2 Heulof-aer gm-eu-aq,
\q1 Aqloq-euq uqduq-anr nae maeqmanq-anr yoerka lef-awr,
\q1 Aqyawf-euq paerkhanq lanrbav-anr yoerlaef-euq,
\q1 Arpoeq khav-eu tsir-anr dur-eu mae.
\q1
\v 3 Heulof-aer gm-eu-aq, Heqlaqmoq gawqjawq-anr ga-eu ceuq,
\q1 Ziroq gawqjawq-euq khoertav gale-euq gar dur-eu mae.
\q1 Aqjeq mirnae aernar-aq,
\q1 Nuima gm-eu gar, Ya’we Miqyaer guiqlanq-euq dzoeqdzaq mr-eu mae.
\q1 Mqmaq maqbyav yarzir daef-eu guiqlanq bif-eu mae.
\c 134
\s1 Uqcif Paw Jaceuq-eu Cardawq
\d Ledav ngae car-eu dawqcar
\q1
\v 1 Hawsher-eq! Nawmaf, Ya’we Miqyaer-euq kuirlaf zaqkhav dawqtawvluf,
\q1 Ya’we Miqyaer-anr jaceuq-awq!
\q1 Uqcif paw, Ya’we Miqyaer-euq nymr-euq lafkhoer yawfluf-eu deuq,
\q1 Aqyawf-anr jaceuq-awq.
\q1
\v 2 Yawshawr jawrduq-euq lafkhoer aqlaf bafdir-awr,
\q1 Ya’we Miqyaer-anr jaceuq-awq!
\b
\q1
\v 3 Mq hawr-eu mir-anr bae-euq Ya’we Miqyaer,
\q1 Ziroq-anr nae nawmaf-anr guiqlanq bif luf-ur!
\c 135
\s1 Ya’we Miqyaer Muiq-eu Gar Jaceuq-eu
\q1
\v 1 Haleluya!
\q1 Aqyawf-anr jaceuq-awq,
\q1 Ya’we Miqyaer-euq kuirlaf zaqkhav deuq-or!
\q1 Ya’we Miqyaer-euq tsawrmyanr-anr jaceuq-awq.
\q1
\v 2 Nawmaf, Ya’we Miqyaer-euq nymr-euq lafkhoer yawfluf-eu deuq,
\q1 Aqdui-euq Miqyaer-euq nymr-euq povka jawr-eu deuq,
\q1
\v 3 Ya’we Miqyaer-aq yawmuiq mirnae,
\q1 Ya’we Miqyaer-anr jaceuq-awq;
\q1 Aqyawf nuikhanr shir-eu mirnae,
\q1 Car-awr aqyawf-euq tsawrmyanr-anr jaceuq-awq.
\q1
\v 4 Aqjeq mirnae aernar-aq,
\q1 Ya’we Miqyaer, aqyawf-euq gharduq Yaqkov-anr tserdov-awr,
\q1 Aqyawf-euq poeq-eu myawqganq pyeufni,
\q1 Irsaqleqlav-anr tserdov-euq mae.
\b
\q1
\v 5 Aqjeq mirnae aernar-aq,
\q1 Ya’we Miqyaer-aq, huiq-awr manr-eu mae laer ngar siqnya ma;
\q1 Aqdui-euq Sanqpaq Miqyaer-aq,
\q1 Miqyaer nuideuq dawqtawv luf-euq khoertav dzaeq-eu mae.
\q1
\v 6 Mq hawr-eu mir-euq khoertav khawf,
\q1 Iqmalavshav hawr-eu naf-eu iqmalavshav dawqtawv luf-euq lafkhoer khawf,
\q1 Ya’we Miqyaer, aqyawf nuima tsaq-eu jeq mr-eu mae.
\q1
\v 7 Aqyawf, mqdzer mirdzer-anr nae ircuf-euq safkhoeq bi dovlar-awr,
\q1 Uqyaer-euq gharduq mqjeq tsawqtaev bitaev-eu uqtaf,
\q1 Aqyawf-euq myawqganq jeqhmq duq-anr nae jaqlaer bi dovle-eu mae.
\b
\q1
\v 8 Aqyawf nae Irjirtuv-euq huqbaw zaqyo hawr-eu huqbaw jeqzaq-anr lavsaef-euq mae.
\q1
\v 9 Irjirtuv-or! Aqyawf, nawq-euq ghanrcae bisiq bimawr-eu jeq hawr-eu zanqler paq-aer ghae-eu jeq,
\q1 Paqlawr hawr-eu aqyawf-euq kuirlaf zaqkhav dawqtawv luf-euq khoertav bi keuvle-euq mae.
\q1
\v 10 Aqyawf, ghawqmya jeuq ghawqmya ca-anr ghaqler-awr,
\q1 Baoq huiq-eu sanqpaq deuq-anr saef-euq mae.
\q1
\v 11 Aqmorliv-euq sanqpaq Sirhoq, Baqshaq-euq sanqpaq Awrgav hawr-eu,
\q1 Kaqnaq mirkhanq-anr jawr-eu sanqpaq dawqtawv luf-anr saef-euq mae.
\q1
\v 12 Khoernar-aq, aqyawf maf-euq mirtsaq, jeqbir lof-aer bif-euq mae;
\q1 Aqyawf-euq tsawrhaq pyeuf-eu Irsaqleqlav-anr jeqbir lof-aer bifnaeq-euq mae.
\b
\q1
\v 13 Ya’we Miqyaer-or!
\q1 Nawq-euq tsawrmyanr mqmaq maqbyav yarzir zif-awr,
\q1 Ya’we Miqyaer-or!
\q1 Tiqzir sair tiqzir saq-eu maqbawni, nawq-euq tsawrmyanr jav taef-a.
\q1
\v 14 Ya’we Miqyaer, aqyawf-euq tsawrhaq-anr yarkhaqdar ngae,
\q1 Aqyawf, aqyawf-euq kuirlaf zaqkhav-anr ghawqshaqgaq taef-a.
\q1
\v 15 Aqcawq tsawrjeuq-euq tanqpanq aqma mawrkawr-aq,
\q1 Pyur hawr-eu shuir yur-awr tsawrhaq-euq aqlaf nae taev-euq taef mae.
\q1
\v 16 Aqyawfmaf khaqmae tsof-eu khawf maq ngaeqnya-aer,
\q1 Myavnuiv tsof-eu khawf maq mawrnya-a.
\q1
\v 17 Aqyawfmaf naqbawr tsof-eu khawf maq gaqnya-aer,
\q1 Khaqmae-anr nae saf khawf maq ghawrnya-a.
\q1
\v 18 Aqyawf maf-anr taev-euq deuq-aq, aqyawf maf-anr durlar taef-a;
\q1 Aqyawf maf-anr kharghoe-eu deuq khawf, aqyawf maf-anr durlar taef-a.
\b
\q1
\v 19 Irsaqleqlav-euq nymrkhanq-or!
\q1 Ya’we Miqyaer-anr jaceuq-awq!
\q1 Aqloq-euq nymrkhanq-or!
\q1 Ya’we Miqyaer-anr jaceuq-awq!
\q1
\v 20 Leqbir-euq Nymrkhanq-or!
\q1 Ya’we Miqyaer-anr jaceuq-awq!
\q1 Ya’we Miqyaer-anr guv-eu nawmaf,
\q1 Ya’we Miqyaer-anr jaceuq-awq!
\q1
\v 21 Ziroq-anr nae Ya’we Miqyaer-anr jaceuq-awq.
\q1 Yeqlursaqlaeq-euq lafkhoer jawr-eu ghaq-anr jaceuq-awq!
\q1 Haleluya!
\c 136
\s1 Guiqlanq-anr Noeqkeuv-awr Jaceuq-eu
\q1
\v 1 Ya’we Miqyaer-euq guiqlanq-anr jaceuq-awq!
\q1 Aqjeq mirnae aernar-aq, aqyawf-aq yawmuiq mae!
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 2 Miqyaer deuq-euq khoertav Miqyaer pyeuf-eu ghaq-euq guiqlanq-anr jaceuq-awq!
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 3 Sanqpaq deuq-euq khoertav Ya’we Miqyaer pyeuf-eu ghaq-euq guiqlanq-anr jaceuq-awq!
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\b
\q1
\v 4 Aqyawf tiq ghaq taef-aer yawkhav-aer zanqler paq-aer ghae-eu jeq mr-eu mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 5 Aqyawf-euq zaqsiq tsawrganr nae, mqdeuq-anr bae-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 6 Aqyawf nae ircuf-euq khoertav, mirtsaq ciqkhawq taq-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 7 Aqyawf nae yawshaef yawhuiq deuq-anr bae-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 8 Nanrma-aq, nan’ghanr paw bi uvlanr-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 9 Bala hawr-eu aqguir deuq-aq, uqcif paw bi uvlanr-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\b
\q1
\v 10 Aqyawf, Irjirtuv-euq huqbaw deuq-anr saef-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 11 Khoernar-aq, aqyawf maf-euq ghanrcae nae, Irsaqleqlav-anr sheuqdov laf-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 12 Aqyawf-euq lafdu cawqja-awr ghaq khav-eu aqlaf nae sheuqdov-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 13 Aqyawf nae Irner iqmalavshav nyiqpav-aer birpav-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 14 Khoernar-aq, Irsaqleqlav tsawrhaq-anr heu-euq ghanrcae nae bitsaev-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 15 Aerkhawf, Paqlawr hawr-eu aqyawf-euq guirmaf-anr, Irner iqmalavshav-anr daeqpuf naeq-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 16 Aqyawf, aqyawf-euq tsawrhaq-anr bawrtsanq bawrdanr-anr sheuqtsaev naeq-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 17 Aqyawf, huiq-eu sanqpaq deuq-anr saef-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 18 Tsawrmyanr dov-euq sanqpaq-anr khawf saef-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 19 Aqmorliv-euq sanqpaq Sirhoq-anr saef-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 20 Baqshaq-euq sanqpaq Awrgav-anr khawf saef-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 21 Khoernar-aq, aqyawf maf-euq mirtsaq jeqbir lof-aer bif-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 22 Aqyawf-euq kuirlaf zaqkhav Irsaqleqlav-anr jeqbir lof-aer bif-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\b
\q1
\v 23 Aqdui teq-awr nyir-eu artiv-anr jawr ngae, aqyawf, aqdui-anr noeqkeuv naeqlaf-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 24 Aqdui-anr muir-eu deuq-anr nae, yawkawr-aer gheudov naeqlaf-euq mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\q1
\v 25 Aqyawf, shiqshaq dawqtawv luf-anr dzaqdzaq dawrdawr bif-eu mae.
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\b
\q1
\v 26 Mqtav-euq Miqyaer-euq guiqlanq-anr jaceuq-awq!
\q1 Aqyawf-euq ghawqshaq gaq-eu jeq, mqmaq maqbyav yarzir zif-eu mae.
\c 137
\s1 Ziroq-anr Noeqmawr-eu Cardawq
\q1
\v 1 Nga’maf, Baqburloq lawrgawv gawvdzer nuir-awr,
\q1 Ziroq-anr noeqmawr-eu mirnae ngoer-euq ma.
\q1
\v 2 Nga’maf, khoergar daef-eu miqshuq arbawr-anr,
\q1 Nga’maf-euq deur-mq gheuceuq-euq ma.
\q1
\v 3 Khoerngae, zaqkhav bigeuf-ir-eu laer,
\q1 Nga’maf-anr nyaefghae luf-eu deuq,
\q1 Nga’maf-anr cardawq car laer shar-awr,
\q1 Nga’maf-anr tanrjoe daedzaq-euq deuq,
\q1 Nga’maf-anr lurgha caergha-awq laer khaer-eu mae.
\q1 Aqyawfmaf aer-eu,
\q1 “Nga’maf-anr Ziroq-euq cardawq car naeqlaf-awq!”
\b
\q1
\v 4 Nga’maf, boermiq siqkhaq-euq mirtsaq-euq lafkhoer,
\q1 Ya’we Miqyaer-euq cardawq aqjox-aer carnya-eu?
\q1
\v 5 Yeqlursaqlaeq-or!
\q1 Ngar, nawq-anr ngaertsaev law nar-aq,
\q1 Ngaq-euq armar laf mrnya-eu jeq maq mrnya’ni bipyeuf-ir luf-ur.
\q1
\v 6 Ngar, nawq-anr maq noeqmawr-aer, yawqjeqyawq navmaqtser,
\q1 Ngar, Yeqlursaqlaeq-anr laqkhaq maq caerdzaeq nar-aq,
\q1 Ngaq-euq maeqlar khaqmae maeqtav paw myanqler-awr khav-ir luf-ur.
\b
\q1
\v 7 Ya’we Miqyaer-or!
\q1 Yeqlursaqlaeq bi deuvdzaq ngae nan,
\q1 Eqdoq tsawrhaq deuq nae aer-euq jeq-anr taqngaer naeqlaf.
\q1 Aqyawfmaf aer-eu,
\q1 “Meuq teumeuq deuvpyav jir-awq, dawqpaeq duqpur-anr keuvni deuvpyav jir-awq!”
\q1
\v 8 Baqburloq-euq zaqmiq-or! Nawr yabi lavcawv taef-a.
\q1 Nawq nae nga’maf-anr janrnaeq laf-euq lof-aer, nawq-anr par janrkhof naeq-eu tsawrhaq-aq, guiqlanqhuiq taef-a.
\q1
\v 9 Nawq-euq zaq yawzaq deuq-anr nyaef-awr, khaqlo-anr juifghae-eu tsawrhaq-aq, guiqlanqhuiq taef-a.
\c 138
\s1 Nuima Dawqtawvluf Jaceuq-eu
\d Daqbir-euq cardawq
\q1
\v 1 Ngar, ngaq-euq nuima dawqtawvluf nae nawq-anr ciqpoeq ciqgor ma.
\q1 Ngar car-awr nae miqyaer nuideuq-euq maeqshi, nawq-anr jaceuq ma.
\q1
\v 2 Nawq-euq ghoer-awr gaq-eu jeq hawr-eu nawq-euq yawtsaq yawdawr-euq baqda’nae,
\q1 Nawr, nawq-euq tsawrmyanr dawqtawv luf-euq khoertav nawq-euq dawq-anr taqhuiq taq-euq baqda’nae,
\q1 Ngar, nawq-euq shawr-eu uqduq tanqnymr nymrma jav-eu paw ciqpyawr-awr uqduq tanq-eu lafkhoer, nawq-euq tsawrmyanr-anr jaceuq ma.
\q1
\v 3 Ngar, nawq-anr kur ngae nan-anr nawr, ngaq-euq dawq narhaq naeqlaf-awr,
\q1 Ngaq-euq nuima saflar ghaq bi anrlar-awr khaqduqni bi ghaelar naeqlaf-euq mae.
\b
\q1
\v 4 Ya’we Miqyaer-or! Mirkhanq sanqpaq deuq, nawq-euq khaqmae-anr nae dovle-euq dawq yagaq ngae,
\q1 Aqyawfmaf dawqtawvluf, nawq-anr jaceuq taef-a.
\q1
\v 5 Aqyawfmaf, Ya’we Miqyaer-euq garma-euq garkawv car taef-a;
\q1 Aqjeq mirnae aernar-aq, Ya’we Miqyaer-euq yawshaef khanqsaer-aq, huiq-awr manr-eu mae.
\q1
\v 6 Ya’we Miqyaer-aq, yawgor-anr jawr-eu khawf,
\q1 Aqyawf, yaw’aw-anr jawr-eu deuq-anr noeqkeuv-eu mae;
\q1 Aerkhawf, nuima huiq-eu deuq-anr-aq, garmanr kaq-eu gar nae siqnya-eu mae.
\b
\q1
\v 7 Ngar gardzaq garkawv-euq ghanrcae canluf khawf,
\q1 Nawr, ngaq-anr tiq pov par lavdaef naeqlaf taef-a;
\q1 Nawr, nawq-euq aqlaf cawqja-awr nae, ngaq-anr muir-eu deuq-euq nuima paer-eu gar caekhofnaeq taef-a.
\q1 Khoernar-aq, nawq-euq armar laf, ngaq-anr bibuq-ir naeqlaf taef-a.
\q1
\v 8 Ngaq-anr taf-eu garkawv deuq-aq, Ya’we Miqyaer mrbyan taef-a.
\q1 Ya’we Miqyaer-or! Nawq-euq ghawqshaq gaq-euq jeq mqmaq maqbyav yarzir zif-eu mae.
\q1 Nawq-euq aqlaf-euq gardeuq-anr taq ngaerbyov naeqlaf.
\c 139
\s1 Miqyaer, Tsawrhaqzaq-anr Siqnya-eu
\d Car-eu paw sheuqghae-eu aqyuif ghaq-anr bofnaeq-euq Daqbir-euq cardawq
\q1
\v 1 Ya’we Miqyaer-or! Nawr, ngaq-anr povtser duq-awr, ngaq-anr siqnya-eu mae.
\q1
\v 2 Nawr-aq, ngar nuir ngae hawr-eu yawf ngae khawf siqnya-eu mae;
\q1 Nawr garmanr kaq-eu gar jawr-awr, ngaq-euq nuinoeq ghovshaw nya-eu mae.
\q1
\v 3 Nawr-aq, ngaq-euq garma siqnya-awr, ngar ghaqnaq naq-eu khawf siqnya-eu mae;
\q1 Nawr, ngaq-euq garma dawqtawv luf-anr siqnya-eu mae.
\q1
\v 4 Aqjeq mirnae aernar-aq, ngaq-euq maeqlar tiqdawq-iq maqngaeq-euq khawf;
\q1 Ya’we Miqyaer-or! Haeq, nawr siqnya jir-eu mae.
\q1
\v 5 Nawr, ngaq-euq maeqhu maeqnan-aer tiqjawq jawr-awr,
\q1 Nawq-euq aqlaf, ngaq-euq khoertav bafdav taq-euq mae.
\q1
\v 6 Siqnya-eu tsawrganr heu-aq, ngaq-euq gharduq ardzaeq-aer zanqler paq-aer ghae-awr,
\q1 Ngar maq yurmir nya-eu gar yawgor-anr jav-eu mae.
\b
\q1
\v 7 Ngar, nawq-euq Saflar-anr nae aqgeugar irbyov nya-eu?
\q1 Ngar, nawq-euq maeqshi nae aqgeugar caevbyov nya-eu?
\q1
\v 8 Ngar, mq-anr davler nar-aq, nawr khoergar jawr-eu mae;
\q1 Ngar, ngaq-euq yufzar shiqbuir lanma-anr taq-ir nar-aq khawf, nawr khoergar jawr taef-a.
\q1
\v 9 Ngar, uqshawq-euq aqdanr-anr dziq-awr,
\q1 Kaq duidui-eu iqmalavshav-anr jawrbyov-ir-eu law-awr khawf,
\q1
\v 10 Khoergar, nawr, nawq-euq armar laf nae ngaq-anr nyaeftaeq-awr,
\q1 Nawq-euq aqlaf nae, ngaq-anr sheuqghae naeqlaf taef-a.
\q1
\v 11 “Yawzanr, ngaq-anr yawduq bawni dzaekaqle taef-a” laer ngar aernar-aq,
\q1 Uqcif khawf, ngaq-euq tiqjawq yawshaef byalar taef-a.
\q1
\v 12 Ngeur mae. Yawzanr-aq, nawq-anr nae maq caevzaf nya-aer;
\q1 Uqcif khawf, nan’ghanr paw lof-aer yawshaef bya-eu mae.
\q1 Nawq-euq gharduq-aq, yawzanr hawr-eu yawbya nyiq jeqluf tiqgar dur-eu mae.
\b
\q1
\v 13 Aqjeq mirnae aernar-aq, ngaq-euq lafkhoer anr-eu jeq-aq, nawq nae shaev-awr,
\q1 Ngar, ngaqma-euq uqmav-euq lafkhoer jawr ngae,
\q1 Nawr, ngaq-anr dzeuf-euq mae.
\q1
\v 14 Ngar, nawq-anr jaceuq ma.
\q1 Aqjeq mirnae aernar-aq,
\q1 Nawr, ngaq-anr guvlar paq-aer hawr-eu zanqler paq-aer bae naeqlaf-euq mae.
\q1 Nawq nae mr-euq gardeuq-aq, muiq duidui-eu mae.
\q1 Ngaq-euq nuima saflar, yawmuiq paq-aer siqnya ma.
\q1
\v 15 Mirtsaq-euq dawqdan, ngaq-anr yawmuiq-aer shaev-awr dzeuf-eu lafkhoer,
\q1 Yawzaf zaf-aer ghae-eu gar ngaq-anr bae ngae,
\q1 Ngaq-euq mawrkawr-aq, nawq-anr nae zaftaq-euq maqngeur-a.
\q1
\v 16 Ngaq-euq mawrdo-euq mawrkawr, maq kawrghaq-euq minav-aer,
\q1 Nawq-euq myavnuiv, ngaq-anr mawrluf-eu mae.
\q1 Kharngeur-eu jeq maq pyeuflar-euq minav-aer,
\q1 Ngar yadaef yajawr-eu arnan aryamq deuq khawf,
\q1 Dawqtawvluf, nawq-euq sanqbof-euq lafkhoer bofanr taq-euq mae.
\q1
\v 17 Miqyaer-or! Nawr, ngaq-euq gharduq sharnoeq naeqlaf-eu jeq poeq-eu gar nae-aq;
\q1 Kharngeur-eu jeq sawxluiv ngae, arnavni huiq-eu nga!
\q1
\v 18 Ngar, nawq-euq nuinoeq heudeuq gui-eu le laer nar-aq,
\q1 Kharshuif myavmaqtser myaq-eu gar ya’mawr taef-a.
\q1 Ngar noeqlar ngae khawf, nawq naeq-aer jawr ma.
\b
\q1
\v 19 Miqyaer-or! Nawq nae tsawrcawv-anr bi saefdzaemawf mir-a.
\q1 Nawmaf, shiq maef-eu tsawrhaq deuq-or!
\q1 Ngar jawr-eu gar nae dov-ir-awq.
\q1
\v 20 Aqjeq mirnae aernar-aq,
\q1 Aqyawfmaf, nawq-euq garkawv jadoer jacawv-awr,
\q1 Nawq-anr muir-eu deuq, nawq-euq tsawrmyanr zmqcawv-eu mae.
\q1
\v 21 Ya’we Miqyaer-or!
\q1 Nawq-anr byae-eu deuq-anr, ngar maqbyae-eu nga laq?
\q1 Nawq-anr muir-eu laer turlar-euq deuq-anr, ngar nuidav maqdav-eu nga lor?
\q1
\v 22 Ngar, aqyawf maf-anr aerkhar maqza’ni byae-awr,
\q1 Ngar, aqyawf maf-anr ngaq-anr muir-eu deuq laer sawxtaq-euq ma.
\q1
\v 23 Miqyaer-or! Ngaq-anr pyaevtser-awr, ngaq-euq nuima siqnya-awq;
\q1 Ngaq-anr tsermr-awr, ngaq-euq nuinoeq-anr khawf siqnya-awq.
\q1
\v 24 Khoernar-aq, ngaq-euq lafkhoer yawcawv-euq garma anr-eu nga laq laer haw-awr,
\q1 Ngaq-anr mqmaq maqbyav yarzir-euq garma-anr sheuqghae naeqlaf-awq.
\c 140
\s1 Tsawrdoer-anr Nae Bibuq-ir Naeqlaf
\d Car-eu paw sheuqghae-eu aqyuif ghaq-anr bofnaeq-euq, Daqbir-euq cardawq
\q1
\v 1 Ya’we Miqyaer-or!
\q1 Tsawrdoer deuq-anr nae, ngaq-anr bibuq-ir laf;
\q1 Coeqlar deuqlarni mr-eu deuq-anr nae, ngaq-anr bidzeur-ir naeqlaf.
\q1
\v 2 Aqyawfmaf, yawqha-euq nuima-euq lafkhoer bawqtov yawdoer jeq tov-awr;
\q1 Aqyawfmaf, aqmyanluf guirdeuv deuv-eu laer tiqkawv lof-aer uiqler-eu mae.
\q1
\v 3 Aqyawfmaf, yawqha-euq maeqlar arlawr-euq maeqlar lof-aer lavtav-awr;
\q1 Aqyawf maf-euq maeqluq-euq lafkhoer paw, dof khav-eu arlawr uirnav-euq dof anr-eu mae.
\qs Sela.\qs*
\b
\q1
\v 4 Ya’we Miqyaer-or!
\q1 Tsawrcawv-euq aqlaf-anr nae, ngaq-anr hawpir naeqlaf;
\q1 Coeqler deuqler-eu jeq mr-eu deuq-anr nae, ngaq-anr bidzeur-ir laf.
\q1 Aqyawfmaf, ngaq-euq kuirtof sanqtif bitif-ir le laer noeqtsov taq-euq mae.
\q1
\v 5 Nuima huiq-eu tsawrhaq, ngaq-anr lanrju hawr-eu cavpyeuv khmrzaf taq-euq mae;
\q1 Aqyawfmaf, garma gardzer bawghaf khmrtaq-awr,
\q1 Ngaq-anr lafpeu khmrtaq-euq mae.
\qs Sela.\qs*
\b
\q1
\v 6 Ngar, Ya’we Miqyaer-anr heulof-aer aernaeq ma:
\q1 “Ya’we Miqyaer-or! Nawr-aq, ngaq-euq Miqyaer ma.
\q1 Ngar tanqshar-eu tersar-anr narhaq naeqlaf.
\q1
\v 7 Ngaq-anr kakhof-eu ghaq khav-eu Sanqpaq Miqyaer pyeuf-eu Ya’we Miqyaer-or!
\q1 Guirdeuv deuv-eu arnan-anr nawr, ngaq-euq uqduq-anr daeqkaq naeqlaf-euq mae.
\q1
\v 8 Ya’we Miqyaer-or!
\q1 Tsawrcawv deuq geufmawf-eu jeq taq bifnaeq;
\q1 Aqyawfmaf yawdoer jeq tovtaq-euq khoer, taqbi irganrnya law;
\q1 Maqngeur nar-aq, aqyawfmaf khanqsaer jaev-eu baw-mr.
\qs Sela.\qs*
\b
\q1
\v 9 “Ngaq-anr gajawq taq-euq deuq-euq uqduq-anr-aq,
\q1 Aqyawf maf-euq maeqluq-euq yawdoer jeq nae gadav luf-ur;
\q1
\v 10 Miqdzaq dofluf-eu miqdziv, aqyawf maf-euq khoertav bi gale-awr;
\q1 Aqyawf maf-anr tiqjawr-iq maq ghofdovlar nya’ni,
\q1 Naf-eu duqkhanq-euq lafkhoer, miqdzaq-euq lafkhoer dzae’anr ghae-awq.
\q1
\v 11 Aqcawq-anr jabu jadoer-eu tsawrhaq-aq, mirtsaq-euq khoertav taqbi ziflar;
\q1 Yawdoer jeq shaqtae tae-eu lof-aer, coeqler deuqlerni mr-eu tsawrhaq-anr taemi luf-ur.”
\b
\q1
\v 12 Ya’we Miqyaer, bikhaeq-euq deuq-anr hawpir naeq-awr,
\q1 Irshaqzaq-anr dawr-awr tsaq-eu jeq janrnaeq mae laer ngar siqnya ma.
\q1
\v 13 Yawduq bawni, tsaq-awr dawr-eu tsawrhaq-aq, nawq-euq tsawrmyanr-anr jaceuq-awr,
\q1 Dawrcoeni daef-eu tsawrhaq-aq, nawq-euq maeqshi yajawr taef-a.
\c 141
\s1 Tsawrdoer-anr Nae Bibuq-ir Naeqlaf
\d Daqbir-euq cardawq
\q1
\v 1 Ya’we Miqyaer-or!
\q1 Ngar, nawq-anr kur-eu ma.
\q1 Ngar, nawq-anr kur-eu gar,
\q1 Ngaq-euq tersar-anr naqbawr ciqkhmr-awr narhaq naeqlaf-awr;
\q1 Ngar jawr-eu gar yawkawr-aer oe naeqlaf-awq.
\q1
\v 2 Ngaq-euq guiqlanq shardawq, nawq-euq maeqshi dofdamq-euq jeq lof-aer bipyeuf-awr;
\q1 Ngaq-euq aqlaf bafdir-eu-aq, uqcif uqtaw paw lawr-eu jeq lof-aer bi pyeufnaeq laf-awq.
\b
\q1
\v 3 Ya’we Miqyaer-or!
\q1 Ngaq-euq khaqmae-anr hawlanr naeqlaf-awr,
\q1 Ngaq-euq maeqluq-euq larghov-anr hawpir naeqlaf-awq.
\q1
\v 4 Ngar, yawlaeq yawca jeq mr-eu tsawrhaq naeq-aer yawcawv jeq mr-ir paq-aer,
\q1 Ngaq-euq nuima yawdoer jeq tiqjeq-anr-iq taqbi pyawr-ir naeqlaf.
\q1 Khoeruqtaf, aqyawfmaf dzaq-eu dzarkuir deuq taqbi dzaq-ir naeqlaf.
\b
\q1
\v 5 Tsaq-awr dawr-eu tsawrhaq nae ngaq-anr bidiq-awq;
\q1 Khoer-aq, ghawqshaq gaq-eu jeq pyeuf taef-a.
\q1 Aqyawf nae ngaq-anr bidaeq-awq;
\q1 Khoer-aq, zawr muiq-eu tsir lof-aer pyeuf-awr;
\q1 Ngaq-euq uqduq maq hageuf-aer taqbi ghae naeqlaf.
\q1 Aqjeq mirnae aernar-aq,
\q1 Ngar guiqlanq sharshar-aer tsawrcawv deuq-euq gardeuq-anr deuvluf-eu ma.
\q1
\v 6 Aqyawf maf-euq yarkhaq dar-eu tsawrhaq-aq, baqghapyaf-anr nae bi daeqka ghae-awr;
\q1 Ngaq-euq dawqdeuq yawcoer mirnae, aqyawfmaf yagaq taef-a.
\q1
\v 7 Tsawrhaq nae mirtsaq duqpoq-eu laer duqtaq-euq lof-aer;
\q1 Nga’maf-euq shaqyoeq, lmqbymr maeqkhaeq-anr tiqgar tiqhmq-aer galuf-eu mae.
\b
\q1
\v 8 Aerkhawf, Sanqpaq Miqyaer pyeuf-eu Ya’we Miqyaer-or!
\q1 Ngaq-euq myavnuiv-aq, nawq-anr hawtsov taq-euq ma;
\q1 Ngar, nawq-euq lafkhoer caevzaf ma;
\q1 Ngaq-euq nuima saflar-anr taq byaerdzae naeqlaf.
\q1
\v 9 Ngaq-anr aqyawfmaf nae khmrtaq-euq lanrju-anr taqbi pafler naeqlaf.
\q1 Yawlaeq yawca jeq mr-eu deuq-euq yadmq-anr nae bidzeur-ir laf.
\q1
\v 10 Ngar yabuq yalaw-ir luf ngae,
\q1 Tsawrcawv deuq, yawqha nae khmrtaq-euq lanrju-anr bi paflerjir luf-ur.
\c 142
\s1 Miqyaer-anr Gurgaq-eu
\d Daqbir-euq Maqsaqkir\f + \fr 142:0 \fk Maqsaqkir \ft Car-eu paw-euq dawq tiq dawq, dzardziv dziv-eu laer aermawf-eu mae.\f*, aqyawf khaqlo boe-anr\f + \fr 142:0 \fk Khaqlo boe-anr \ft Garkawv heukawv-aq, 1Sml 22:1; 24:3 lafkhoer anr-eu mae.\f* jawr ngae-euq guiqlanq shardawq
\q1
\v 1 Ngar, ngaq-euq tersar nae Ya’we Miqyaer-anr gurgaq ma.
\q1 Ngar, ngaq-euq tersar nae Ya’we Miqyaer-anr tanqshar-eu dawqdzawr mr ma.
\q1
\v 2 Ngar nuikhanr maqshir-eu jeq, aqyawf-euq maeqshi shaevdov lef-awr;
\q1 Ngaq-euq gardzaq garkawv, aqyawf-euq maeqshi dzawrdov-euq ma.
\q1
\v 3 Ngaq-euq saflar, ngaq-euq lafkhoer ghawqshaq-aer ghae-ir-awr javluf ngae,
\q1 Nawr, ngaq-euq cankawv siqnya-eu mae.
\q1 Ngar can-eu garma-anr, aqyawfmaf nae lanrju khmrzaf taq-euq mae.
\b
\q1
\v 4 Ngar, ngaq-euq armar paw hawghae ngae,
\q1 Haeqsher-eq, ngaq-anr tiqghaq-iq maq siqnya-a;
\q1 Ngar caevzafduq maqpar jav-aer,
\q1 Ngaq-euq nuima saflar-euq garkawv sharnoeq naeqlaf-eu tiqghaq-iq maqjawr-a.
\b
\q1
\v 5 Ya’we Miqyaer-or! Ngar, nawq-anr gurgaq-eu ma.
\q1 Ngar heulof-aer aer ma:
\q1 “Nawr-aq, ngaq-euq caevzafduq pyeuf-awr,
\q1 Yawdaefzaq jawr-eu mirtsaq-euq khoertav, ngaq-euq shaqbymr ma.
\q1
\v 6 Ngar gur-eu gar narhaq naeqlaf-awq.
\q1 Aqjeq mirnae aernar-aq,
\q1 Ngar ardzaeq-aer aw-eu gar bi gheuka taq-euq nya;
\q1 Ngaq-anr khaeq-awr khav-eu deuq-anr nae bibuq-ir naeqlaf-awq.
\q1 Aqjeq mirnae aernar-aq,
\q1 Aqyawfmaf, ngaq-euq navmaqtser ghaq khavdzaeq-eu mae.
\b
\q1
\v 7 Ngar, nawq-euq tsawrmyanr-anr jaceuq nya’ni,
\q1 Ngaq-euq nuima saflar, kuv-anr nae sheudov naeqlaf-awq;
\q1 Nawr, ngaq-anr nuijer jer-awr guiqlanq bif-eu mirnae,
\q1 Tsaq-awr dawr-eu tsawrhaq deuq, ngaq-euq tiqjawq uiqlar taef-a.”
\c 143
\s1 Ngaq-anr Lavdaef Naeqlaf-awq
\d Daqbir-euq cardawq
\q1
\v 1 Ya’we Miqyaer-or!
\q1 Ngaq-euq guiqlanq shardawq-anr narhaq naeqlaf-awq.
\q1 Ngar tanqshar-eu jeq-anr nawq-euq naqbawr ciqkhmr naeqlaf-awq.
\q1 Nawq-euq sarjax zif-awr, tsaq-awr dawr-eu lafkhoer, ngaq-anr dawq ngaeqkhof naeqlaf-awq.
\q1
\v 2 Nawq-euq kuirlaf zaqkhav-anr yarkhaq taq darnaeqlaf.
\q1 Aqjeq mirnae aernar-aq,
\q1 Nawq-euq maeqshi tsaq-awr dawr-eu tsawrhaq tiqghaq-iq maqjawr-a.
\b
\q1
\v 3 Muir-eu ghaq, ngaq-euq nuima saflar-anr khaeq-awr khavnaeq laf-awr,
\q1 Ngaq-euq daefzir mirtsa nawqpyaev naeqlaf-euq mae;
\q1 Aqyawf, ngaq-anr arhuba nae shirtaq-euq deuq-anr lof-aer,
\q1 Zanrtsuini ghae-eu lafkhoer bijawr naeqlaf-euq mae.
\q1
\v 4 Khoermirnae, ngaq-euq lafkhoer, ngaq-euq saflar ghaq juq-ir-awr,
\q1 Ngaq-euq lafkhoer, ngaq-euq nuima shaqzanq mrluf mir-a.
\b
\q1
\v 5 Ngar, arhu-euq arnan aryamq-anr noeqmawr-awr,
\q1 Ngar, nawq nae mr-euq gardeuq dawqtawv luf-euq garkawv dzardziv dziv-eu lafkhoer,
\q1 Ngar, nawq-euq aqlaf nae gar mr-euq garkawv sharnoeq sharsawx luf-eu ma.
\q1
\v 6 Ngar, ngaq-euq aqlaf nawr jawr-eu paw cawqdir laf-eu ma;
\q1 Ngaq-euq nuima saflar, ircuf maef-eu mirtsaq lof-aer, nawq-anr maef mir-a.
\qs Sela.\qs*
\b
\q1
\v 7 Ya’we Miqyaer-or!
\q1 Ngaq-anr yawkawr-aer dawq ngaeqkhof naeqlaf-awq;
\q1 Ngaq-euq saflar, ghaq jirshaw-iq mir-a!
\q1 Nawq-euq myavpyawq, ngaq-anr nae taq ciqzaf naeqlaf,
\q1 Maqngeur nar-aq, ngar duqkhanq-anr ir-eu deuq-anr durlar-eu baw-mr.
\q1
\v 8 Uqshawq paw, ngaq-anr nawq-euq ghoer-awr gaq-eu garkawv bigaq naeqlaf,
\q1 Aqjeq mirnae aernar-aq, ngar, nawq-anr ghoe ma.
\q1 Ngar, can paq-aer ghae-eu garma, ngaq-anr bi siqnya naeqlaf-awq,
\q1 Aqjeq mirnae aernar-aq, ngar, ngaq-euq nuima saflar, nawq-anr ciqgornaeq ma.
\b
\q1
\v 9 Ya’we Miqyaer-or!
\q1 Ngaq-anr muir-eu deuq-anr nae, ngaq-anr bibuq-ir naeqlaf-awq;
\q1 Ngar-aq, nawq-euq lafkhoer jawrzaf ma.
\q1
\v 10 Ngar, nawq-euq nuima tsaq-eu jeq mrnya’ni maeqnaeq laf-awq,
\q1 Aqjeq mirnae aernar-aq, nawr-aq, ngaq-euq Miqyaer ma.
\q1 Nawq-euq Saflar-aq yawmuiq mae.
\q1 Ngaq-anr dawrcoeni ghae-eu deuq-euq mirtsaq-anr sheuqghae naeqlaf-eu mae.
\b
\q1
\v 11 Ya’we Miqyaer-or!
\q1 Nawq-euq tsawrmyanr gharduq, ngaq-anr lavdaef naeqlaf-awq!
\q1 Nawq-euq tsaq-awr dawr-eu jeq-euq baqda’nae,
\q1 Ngaq-euq nuima saflar, gardzaq garkawv-anr nae gheudov naeqlaf-awq.
\q1
\v 12 Nawr, ngaq-anr ghawqshaq gaq-eu lafkhoer,
\q1 Ngaq-anr muir-eu deuq-anr deuvtsaev dzae-awr,
\q1 Ngaq-euq nuima saflar-anr shaqzanq bi keuvle-euq deuq-anr lavcawv jir-awq.
\q1 Aqjeq mirnae aernar-aq,
\q1 Ngar-aq, nawq-euq kuirlaf zaqkhav ma.
\c 144
\s1 Guir Bi Deuvnya Naeqlaf-eu Miqyaer
\d Daqbir-euq cardawq
\q1
\v 1 Ngaq-euq logur pyeuf-eu Ya’we Miqyaer-anr jaceuq ma.
\q1 Aqyawf, ngaq-euq lafnoer guirdeuv deuvnya’ni hawr-eu,
\q1 Ngaq-euq aqlaf deuvcae caenya’ni maeqnya naeqlaf-euq mae.
\q1
\v 2 Aqyawf-aq, ngaq-anr ghoer-awr gaq-eu, ngaq-euq lanrzif pyeuf-awr,
\q1 Ngaq-anr bibuq-ir-eu ghaq, ngaq-euq lofpir-eu nymrgor pyeuf-eu uqtaf,
\q1 Ngar, aqyawf-euq lafkhoer caevzaf ler-eu, ngaq-euq daeqdaqduq mae;
\q1 Ngaq-euq tsawrhaq, ngaq-euq lafov jawrni, aqyawf nae janr-euq mae.
\b
\q1
\v 3 Ya’we Miqyaer-or!
\q1 Nawr hawcawq-awr siqnya paq-aer tsawrhaq aqjeq la?
\q1 Nawr, aqyawf-euq garkawv sharnoeq paq-aer, tsawrhaqzaq teu aqjeq nga?
\q1
\v 4 Tsawrhaqzaq-aq, saf-anr dur-awr,
\q1 Aqyawf-euq arnan aryamq-aq, tiqkhaeq nar-aq byov-ir-eu narshmr-anr dur-eu mae.
\b
\q1
\v 5 Ya’we Miqyaer-or!
\q1 Nawq-euq mq ngawqghm-awr zafka le-awq;
\q1 Gawqjawq-anr toeqciv-awr uqkhoeq dudu-aer bi dovlar-awq.
\q1
\v 6 Mqjeq tsawqtaev bitaev-awr, aqyawf maf-anr taebyav ghae-awq.
\q1 Nawq-euq kavmyaq beuvdov ghae-awr, aqyawf maf-anr lavcawv-awq.
\q1
\v 7 Yawgor-anr nae nawq-euq aqlaf cawqja lef-awr,
\q1 Boermiq siqkhaq-euq aqlaf hawr-eu ardzaeq-aer myaq-eu ircuf-anr nae,
\q1 Ngaq-anr gheudov-awr bibuq-ir naeqlaf-awq.
\q1
\v 8 Aqyawf-euq khaqmae-aq, gardanr maqtsoeq-eu dawq ngaeq-awr,
\q1 Aqyawf-euq armar laf-aq, maershuir shuir-eu jeq mr-eu armar laf mae.
\b
\q1
\v 9 Miqyaer-or! Ngar, nawq-anr cardawq yawshuif carnaeq ma;
\q1 Ngar, tser cav tsof-eu deur-mq diq-awr carcar-aer, nawq-anr jaceuq ma.
\q1
\v 10 Aqyawf-aq, mirkhanq sanqpaq-anr kakhof-eu ghaq pyeuf-awr,
\q1 Aqyawf-euq kuirlaf zaqkhav Daqbir-anr,
\q1 Deuvlar-eu doer-awr cawv-eu micaer-anr nae bibuq-ir-euq ghaq mae.
\q1
\v 11 Boermiq siqkhaq-euq aqlaf-anr nae,
\q1 Ngaq-anr gheudov-awr bibuq-ir naeqlaf-awq.
\q1 Aqyawf-euq khaqmae-aq, gardanr maqtsoeq-eu dawq ngaeq-awr,
\q1 Aqyawf-euq armar laf-aq, maershuir shuir-eu jeq mr-eu armar laf mae.
\b
\q1
\v 12 Nga’maf-euq zaqyo deuq, aqyawfmaf zaqguq ngae,
\q1 Maerlar luf-eu sanqlanq arbawr lof-aer bidur-awr;
\q1 Nga’maf-euq zaqmiq deuq-aq, sanqpaq-euq nymr-anr hawmuiqni,
\q1 Yawmuiq-aer tafmuiq taq-euq khawrzeur lof-aer bidur law.
\q1
\v 13 Nga’maf-euq caerjir-euq lafkhoer, dzaqla ghovcaer kharngeur-eu jeq bibyan-awr;
\q1 Nga’maf-euq yawrdeuq daega-anr gavluf-eu khoer,
\q1 Armyaer armyaer mawr-anr nae bi puqbaer law.
\q1
\v 14 Nga’maf-euq mawrner, mawrzaq ghawqmya’ni bi jawrle-awr,
\q1 Mawrzaq ghaw ngae maqlov maqshav-aer taqbi ghae law,
\q1 Nga’maf-euq garjawq-anr gurgur-aer teuv-eu jeq taqbi jav naeqlaf.
\b
\q1
\v 15 Heulof-aer ghae-eu artiv-anr yajawr-eu tsawrhaq-aq, guiqlanqhuiq taef-a.
\q1 Ya’we Miqyaer, aqyawf maf-euq Miqyaer pyeuf-eu tsawrhaq-aq, guiqlanqhuiq taef-a.
\c 145
\s1 Huiq-awr Muiq-eu Ya’we Miqyaer
\d Daqbir-euq jaceuq-eu dawqcar
\q1
\v 1 Ngaq-euq Miqyaer-or!
\q1 Ngar, nawq-anr ciqpoeq ciqgor ma;
\q1 Ngar, nawq-euq tsawrmyanr-anr, mqmaq maqbyav yarzir, li-eu maqbawni jaceuq ma.
\q1
\v 2 Yawqnan navluf, ngar, nawq-anr jaceuq ma.
\q1 Ngar, nawq-euq tsawrmyanr-anr, mqmaq maqbyav yarzir, li-eu maqbawni jaceuq ma.
\q1
\v 3 Ya’we Miqyaer-aq, huiq-awr manr-eu mae;
\q1 Aqyawf-anr yawkhav-aer jaceuq-eu tsaq mae;
\q1 Aqyawf-euq yawhuiq yawmanr-anr maq povduq nya-a.
\b
\q1
\v 4 Nawq nae mr-euq gardeuq-euq garkawv, tiqzir sair tiqzir jaceuq taef-a;
\q1 Nawq nae baoq-euq lafkhoer mr-euq deuq, dzawrgaqnaeq taef-a.
\q1
\v 5 Ngar, nawq nae mr-euq zanqler paq-aer ghae-eu gardeuq-euq garkawv hawr-eu,
\q1 Nawq-euq jaqlaq yawbya, arnavni yawshaef khanqsaer byan-eu garkawv dzardzivdziv ma.
\q1
\v 6 Tsawrhaqzaq, nawq nae baoq-euq lafkhoer,
\q1 Yawzeu bawlar paq-aer ghae-eu jeq mr-euq garkawv ja taef-a;
\q1 Ngar, nawq-euq yawhuiq yawmanr-euq garkawv dawqdzawr mr ma.
\q1
\v 7 Aqyawfmaf, nawq-euq zawr muiq-eu garkawv noeqmawr-awr ja taef-a;
\q1 Aqyawfmaf, nawr tsaq-awr dawr-eu garkawv car taef-a.
\b
\q1
\v 8 Ya’we Miqyaer-aq, guiqlanq yawdzanr byan-awr, ghoernya gaqnya duidui-eu uqtaf,
\q1 Nuima yawkawr-aer maqpaerni, ghawqshaq yawkhav-aer gaqnya-eu mae.
\q1
\v 9 Ya’we Miqyaer-aq, yawqghaq navluf-anr muiq-awr;
\q1 Aqyawf-euq lafzar-euq khoertav, nanqyuni ghawqshaq gaq-eu mae.
\b
\q1
\v 10 Ya’we Miqyaer-or!
\q1 Nawq-euq lafzar nawq-anr jaceuq-awr;
\q1 Nawq-euq tsawrshawr khawf, nawq-euq guiqlanq-anr jaceuq taef-a.
\q1
\v 11 Aqyawfmaf, nawr uvlanr-eu jeq-euq yawshaef khanqsaer-euq garkawv ja-awr,
\q1 Nawq-euq baoq-euq garkawv ja taef-a.
\q1
\v 12 Khoermirnae, tsawrhaqzaq, baoq-euq lafkhoer aqyawf nae mr-euq garkawv hawr-eu,
\q1 Aqyawf uvlanr-eu jeq-euq jaqlaq yawbya-euq yawshaef khanqsaer siqnyalar taef-a.
\q1
\v 13 Nawr uvlanr-eu jeq-aq, mqmaq maqbyav yarzir zif-eu uvlanr-eu jeq pyeuf-awr;
\q1 Nawq-euq dzoeqdzaq-aq, tiqzir sair tiqzir zif-eu mae.
\b
\q1
\v 14 Ya’we Miqyaer, kharcoq-ir-eu tsawrhaq-anr ciqtur naeq-awr;
\q1 Khar awbyav-eu tsawrhaq-anr ciqgor naeq-eu mae.
\q1
\v 15 Yawdaef khardaef-eu jeq-euq myavnuiv, nawq-anr noeqtmq-awr hawluf-eu mae;
\q1 Nawr yamqlaw tsaq ngae, aqyawf maf-anr dzaqdzaq dawrdawr bif-eu mae.
\q1
\v 16 Nawr, nawq-euq aqlaf pyaeq-awr,
\q1 Yawdaef khardaef-eu jeq-anr, tiqdaev bidzaq bi dawrdaev-eu mae.
\q1
\v 17 Ya’we Miqyaer-euq garma dawqtawvluf tsaq-awr dawr-eu mae;
\q1 Aqyawf kharmr kharshar-eu jeq guiqlanq yawdzanr byan-eu mae.
\q1
\v 18 Ya’we Miqyaer-aq, aqyawf-anr kharkur-eu tsawrhaq deuq-euq dawqpae jawr-eu mae.
\q1 Yawtsaq yawdawr-euq lafkhoer kharkur-eu tsawrhaq-euq dawqpae jawr-eu mae.
\q1
\v 19 Aqyawf, aqyawf-anr guv-eu tsawrhaq deuq geufmawf-eu jeq tiqlof bifnaeq-awr,
\q1 Aqyawfmaf kur ngae, narhaq-awr kakhof naeq-eu mae.
\q1
\v 20 Ya’we Miqyaer, aqyawf-anr khargaq-eu tsawrhaq-anr hawpir-eu mae;
\q1 Aerkhawf, tsawrcawv dawqtawv luf-anr aqyawf lavcawv taef-a.
\b
\q1
\v 21 Ngaq-euq khaqmae, Ya’we Miqyaer-anr jaceuq-eu jeq ngaeq ma;
\q1 Shiqshaq dawqtawvluf mqmaq maqbyav yarzir, li-eu maqbawni;
\q1 Aqyawf-euq shawr-eu tsawrmyanr-anr jaceuq taef-a.
\c 146
\s1 Nuima-or, Ya’we Miqyaer-anr Jaceuq-awq
\q1
\v 1 Haleluya!
\q1 Ngaq-euq nuima saflar-or!
\q1 Ya’we Miqyaer-anr jaceuq-awq!
\q1
\v 2 Ngar yawdaef daefluf ngae,
\q1 Ya’we Miqyaer-anr jaceuq ma;
\q1 Ngar yawdaef khardaef-eu khoerkeuvni,
\q1 Car-awr ngaq-euq Miqyaer-anr jaceuq ma.
\b
\q1
\v 3 Nawr, dzoeqda deuq-anr taqghoe.
\q1 Nawr, maq cawba nya-eu tsawrhaqzaq-anr khawf taqghoe.
\q1
\v 4 Aqyawf maf-euq saf tsaev ngae, mirtsaq-anr parya ghofur-ir-awr;
\q1 Khoerngae nan, aqyawf-euq bawqtov byovjir-eu mae.
\b
\q1
\v 5 Yaqkov-euq Miqyaer nae bi cawba-euq tsawrhaq-aq, guiqlanq huiq-eu mae;
\q1 Aqyawf-euq Miqyaer pyeuf-eu Ya’we Miqyaer-anr noeqtmq-eu tsawrhaq-aq, guiqlanq huiq-eu mae.
\q1
\v 6 Miqyaer-aq, mq khawf, mir khawf, iqmalavshav khawf,
\q1 Heudeuq-euq lafkhoer kharjawr kharjav-eu jeq-anr khawf bae-euq mae.
\q1 Aqyawf, li-eu maqbawni yawtsaq yawdawr-anr nyaeftaeq taq-eu mae.
\q1
\v 7 Aqyawf, bi nyaefsuf taq-euq deuq-euq gharduq,
\q1 Dawr-awr tsaq-eu jeq bi dovlar-awr;
\q1 Maef-eu tsawrhaq-anr dzaqdzaq dawrdawr bifnaeq-awr,
\q1 Ya’we Miqyaer, kuv-anr ur-eu tsawrhaq deuq-anr pyaeqnaeq-eu mae.
\b
\q1
\v 8 Ya’we Miqyaer, myavbaef-anr bi danrlar-awr;
\q1 Ya’we Miqyaer, awbyav-eu tsawrhaq-anr ciqtur naeq-eu mae.
\q1 Ya’we Miqyaer, tsaq-awr dawr-eu tsawrhaq-anr gaq-eu mae.
\q1
\v 9 Ya’we Miqyaer, boermiq siqkhaq deuq-anr hawpir naeq-awr,
\q1 Murcoeq aqnmr hawr-eu miqcoeq miqma deuq-anr ngaeqghae naeq-eu mae.
\q1 Aerkhawf, aqyawf, tsawrcawv-euq garma-anr nanrpov-aer lavpov naeq-eu mae.
\b
\q1
\v 10 Ya’we Miqyaer-aq, mqmaq maqbyav yarzir uvlanr-eu ghaq pyeuf-eu mae;
\q1 Ziroq-or! Nawq-euq Miqyaer yawqzir navluf-anr uvlanr taef-a.
\q1 Haleluya!
\c 147
\s1 Miqyaer-anr Jaceuq-eu Yawmuiq Mae
\q1
\v 1 Haleluya!
\q1 Aqdui-euq Miqyaer-anr car-awr jaceuq-eu-aq yawmuiq mae.
\q1 Jaceuq-eu-aq, tsaqler-awr nuikhanr shirlar-eu mae.
\b
\q1
\v 2 Ya’we Miqyaer, Yeqlursaqlaeq tsovmuiq-awr,
\q1 Aqyawf, bi nyaefbyov ghae-euq Irsaqleqlav-anr sheuquiqlaf luf-eu mae.
\q1
\v 3 Aqyawf, nuima tsawqpav-euq deuq-anr lavtaq naeq-awr,
\q1 Aqyawf maf-euq marnar markhaeq-anr lanrcuf naeq-eu mae.
\b
\q1
\v 4 Aqyawf, aqguir armyav siq bitsaeq-eu laer sawxduq taq-awr,
\q1 Aqyawf nae aqguir yawqsiq navluf-anr kurmyanr biftaq-euq mae.
\q1
\v 5 Aqdui-euq Sanqpaq Miqyaer-aq, huiq-awr manr-eu uqtaf,
\q1 Baoq anr-awr ghaq khav-eu mae.
\q1 Aqyawf-aq, yawdui maqbawni ghovshaw nya-eu mae.
\q1
\v 6 Ya’we Miqyaer-aq, nuinanq nuiyu anr-eu deuq-anr ciqgor naeq-awr;
\q1 Tsawrcawv deuq-anr mirtsa biqkaghae naeq-eu mae.
\b
\q1
\v 7 Ya’we Miqyaer-anr carnaeq-awr,
\q1 Aqyawf-euq guiqlanq-anr jaceuq-awq!
\q1 Deur-mq diq-awr aqdui-euq Miqyaer-anr carnaeq-awr jaceuq-awq.
\q1
\v 8 Aqyawf, mq-anr mqdmr nae gheutsanq-awr,
\q1 Mirtsaq-euq gharduq uqyaer laeqhmq-awr bi yaerle-eu lafkhoer,
\q1 Gawqjawq-anr yarmof bi daeflar-eu mae.
\q1
\v 9 Aqyawf, bawrtsanq-euq jeqzaq-anr hawr-eu,
\q1 Jeqshir khaqdzer-euq zaqdeuq manr ngae,
\q1 Dzaqdzaq dawrdawr bifnaeq-eu mae.
\b
\q1
\v 10 Aqyawf, manqpaq, ghaq khav-eu gar laqkhaq caer-eu maqngeur-aer,
\q1 Aqyawf, tsawrhaq-euq aqkuir-anr nuikhanr shir-eu maqngeur-a.
\q1
\v 11 Ya’we Miqyaer, aqyawf-anr guv-eu tsawrhaq deuq-anr nuikhanr shir-awr,
\q1 Aqyawf ghawqshaq gaq-eu jeq-anr,
\q1 Noeqtmq-eu tsawrhaq deuq-anr nuikhanr shir-eu mae.
\b
\q1
\v 12 Yeqlursaqlaeq-or! Ya’we Miqyaer-anr jaceuq-awq!
\q1 Ziroq-or! Nawq-euq Miqyaer-anr jaceuq-awq!
\q1
\v 13 Aqjeq mirnae aernar-aq,
\q1 Aqyawf, nawq-euq lanrkhaeq-euq ghovkaf-anr lavzif-awr,
\q1 Aqyawf, nawq-euq lafkhoer jawr-eu,
\q1 Nawq-euq zaqdeuq-anr guiqlanq bif-euq mae.
\q1
\v 14 Aqyawf, nawq-euq mirkaq-anr, tsaqler dzoerler-eu jeq bijav-awr;
\q1 Nawq-anr muiq duidui-eu dzaqla ghovcaer tiqlof bifnaeq-eu mae.
\b
\q1
\v 15 Aqyawf, mirtsaq tanqkhawr-anr aqyawf-euq khaerdawq khaerghae ngae;
\q1 Aqyawf-euq dawq yawkhav-aer caevkawr nya-eu mae.
\q1
\v 16 Aqyawf, yawr-euq cafhm lof-aer ghae-eu ngaer bif-awr;
\q1 Aqyawf khaqlaertsar lof-aer ceuqgav byavkhawq ghae-eu mae.
\q1
\v 17 Aqyawf, khaqlo lozaq lof-aer, aqyawf-euq uqyaer arsiq byavka lef-awr,
\q1 Aqyawf nae pyaeqghae lef-euq yawtsaef-anr aqsur ghaq daqcoer-eu.
\q1
\v 18 Aqyawf, aqyawf-euq dawq khaerdovghae ngae, khoerdeuq biguir-ir-awr,
\q1 Aqyawf, jaqlaer bi bawlar-awr ircuf bi yoerle-eu mae.
\b
\q1
\v 19 Aqyawf, Yaqkov-anr aqyawf-euq dawq dawqdzawr mrnaeq-awr,
\q1 Irsaqleqlav-anr aqyawf-euq zanrtaq khoftaq,
\q1 Hawr-eu yarkhaq dar-eu jeq dzawrnaeq-euq mae.
\q1
\v 20 Aqyawf, aqcawq tsawrjeuq-anr heulof-aer maq janrnaeq-aq;
\q1 Aqyawfmaf, Miqyaer-euq yarkhaq dar-eu jeq-anr maq siqnya-a.
\q1 Haleluya!
\c 148
\s1 Mqmir, Miqyaer-anr Jaceuq
\q1
\v 1 Haleluya!
\q1 Mq-anr jawr-awr nae Ya’we Miqyaer-anr jaceuq-awq;
\q1 Yawgor-anr jawr-awr nae aqyawf-anr jaceuq-awq!
\q1
\v 2 Aqyawf-euq mqtav ghaqkhaer dawqtawvluf, aqyawf-anr jaceuq-awq;
\q1 Aqyawf-euq guirmaf dawqtawvluf, aqyawf-anr jaceuq-awq!
\b
\q1
\v 3 Nanrma hawr-eu bala, aqyawf-anr jaceuq-awq;
\q1 Yawshaef-euq aqguir dawqtawvluf, aqyawf-anr jaceuq-awq!
\q1
\v 4 Nawmaf, mqdeuq-euq khoertav jav-eu ircuf hawr-eu,
\q1 Mqdeuq-euq khoertav jav-eu mqdeuq, aqyawf-anr jaceuq-awq!
\b
\q1
\v 5 Heudeuq dawqtawvluf, Ya’we Miqyaer-euq tsawrmyanr-anr jaceuq luf-ur.
\q1 Aqjeq mirnae aernar-aq,
\q1 Aqyawf, dawq bif-awr heudeuq-anr bae-euq mae.
\q1
\v 6 Aqyawf, heudeuq-anr mqmaq maqbyav yarzir,
\q1 Li-eu maqbaw-aer zifni lavzif taq-awr,
\q1 Aqyawf nae maqbyov-ir nya-eu zanrtaq khoftaq biftaq-euq mae.
\b
\q1
\v 7 Mirtsaq-euq khoertav jawr-eu deuq,
\q1 Ya’we Miqyaer-anr jaceuq-awq!
\q1 Nawmaf, huiq-awr manr-eu iqmalavshav-euq jeqzaq,
\q1 Hawr-eu yawnaf-anr kharjawr-eu jeq khawf jaceuq-awq.
\q1
\v 8 Miqdzaq hawr-eu uqyaer arsiq khawf,
\q1 Ngaer hawr-eu mqdmr deuq khawf;
\q1 Aqyawf-euq dawq mrbyan-eu mqjeq jaqlaer khawf jaceuq-awq.
\b
\q1
\v 9 Gawqjawq hawr-eu gawqdur dawqtawvluf;
\q1 Arsiq arbawr hawr-eu siqdaq miqshuq dawqtawvluf khawf jaceuq-awq.
\q1
\v 10 Bawrtsanq-euq jeqzaq hawr-eu cutaq-euq jeqzaq dawqtawvluf khawf,
\q1 Mirtsaq-anr joejoe-aer can-eu jeq hawr-eu,
\q1 Zaw-eu arjir deuq khawf jaceuq-awq.
\b
\q1
\v 11 Mirkhanq-euq sanqpaq deuq hawr-eu tsawrhaqzaq dawqtawvluf khawf;
\q1 Mirkhanq-euq dzoeqda hawr-eu yarkhaq dar-eu dawqtawvluf khawf, jaceuq-awq.
\q1
\v 12 Zaqmiq aqbur hawr-eu zaqyo aqli zaqguq dawqtawvluf khawf,
\q1 Mawq-ir-euq tsawrhaq hawr-eu zaq yawnyir deuq khawf jaceuq-awq.
\b
\q1
\v 13 Aqyawfmaf, Ya’we Miqyaer-euq tsawrmyanr-anr jaceuq luf-ur.
\q1 Aqyawf-euq yawshaef khanqsaer-aq,
\q1 Mq hawr-eu mir navmaqtser gordzaeq-eu mirnae,
\q1 Aqyawf-euq tsawrmyanr-anr taef-aer ciqpoeq ciqgor-eu tsaq mae.
\q1
\v 14 Aqyawf, aqyawf-euq tsawrhaq deuq-euq, ghaq-euq uqcoer ciqgor-eu mae;
\q1 Aqyawf-euq tsawrshawr dawqtawvluf—aqyawf-euq dawqpae jawr-eu tsawrhaq,
\q1 Irsaqleqlav tsawrhaq khawf, aqyawf-anr jaceuq luf-ur.
\q1 Haleluya!
\c 149
\s1 Cardawq Yawshuif Nae Jaceuq-awq
\q1
\v 1 Haleluya!
\q1 Ya’we Miqyaer-anr cardawq yawshuif carnaeq-awq.
\q1 Tsawrshawrzaq uiqler taq-awr jawr-eu gar, aqyawf-anr jaceuq-awq.
\b
\q1
\v 2 Aqyawf maf-anr bae-euq ghaq-euq lafkhoer,
\q1 Irsaqleqlav laqkhaq bicaer luf-ur;
\q1 Aqyawf maf-euq sanqpaq-euq baqda’nae,
\q1 Ziroq-euq zaqdeuq bi lurgha bi caergha luf-ur.
\q1
\v 3 Aqyawfmaf garnyae nyae-awr aqyawf-euq tsawrmyanr-anr jaceuq luf-ur;
\q1 Aqyawfmaf, deur-mq hawr-eu tanqzaq zmq-awr,
\q1 Aqyawf-anr carcar-aer jaceuq luf-ur.
\q1
\v 4 Aqjeq mirnae aernar-aq, Ya’we Miqyaer-aq,
\q1 Aqyawf-euq tsawrhaq-anr nuikhanr shir-eu mae;
\q1 Aqyawf, nuinanq nuiyu anr-eu deuq-anr kakhof-eu jeq nae,
\q1 Hawmuiq paq-aer lavmuiqnaeq taef-a.
\q1
\v 5 Tsawrshawrzaq, yawshaef khanqsaer-euq lafkhoer laqkhaqcaer luf-ur;
\q1 Yawqha-euq yufzar-anr jawr ngae,
\q1 Tersar yawkhav-aer bi cardovlaf luf-ur.
\b
\q1
\v 6 Aqyawf maf-euq aqlaf-anr miseuq nyiq paw tav-eu micaer baf-awr,
\q1 Aqyawf maf-euq khaqmae, Miqyaer-anr yawkhav-aer jaceuq luf-ur.
\q1
\v 7 Aqcawq tsawrjeuq-anr janrdoerkhof naeq-awr,
\q1 Tsawrhaqzaq-anr darnaeq-eu gharduq,
\q1
\v 8 Aqyawf maf-euq sanqpaq-anr shmrcav nae paf-awr,
\q1 Aqyawf maf-euq tsawrhuiq tsawrma-anr,
\q1 Shmr-euq kuirnyaef lafnyaef nae nyaef-eu gharduq;
\q1
\v 9 Aqyawf maf-anr boftaq-euq yarkhaq dar-eu jeq bi pyeuflar-eu gharduq;
\q1 Heu-aq, aqyawf-euq tsawrshawr dawqtawv luf-anr taqhuiq-eu jeq pyeuf-eu mae.
\q1 Haleluya!
\c 150
\s1 Miqyaer-anr Jaceuq-awq
\q1
\v 1 Haleluya!
\q1 Aqyawf-euq shawr-eu jawrduq-euq lafkhoer, Miqyaer-anr jaceuq-awq;
\q1 Aqyawf-euq baoq huiq-eu mq-euq lafkhoer jaceuq-awq!
\q1
\v 2 Aqyawf nae mr-euq huiq-eu gardeuq-euq baqda’nae, aqyawf-anr jaceuq-awq;
\q1 Aqyawf huiqdui manr duidui-eu gar jaceuq-awq!
\b
\q1
\v 3 Darhae-euq tersar nae aqyawf-anr jaceuq-awq;
\q1 Deur-mq zaq hawr-eu deur-mq ma diqdiq-aer jaceuq-awq!
\q1
\v 4 Tanq diq-awr garnyae nyaenyae-aer aqyawf-anr jaceuq-awq;
\q1 Arcav tsof-eu keuka hawr-eu, bawqlaw baw-eu jeq nae jaceuq-awq!
\q1
\v 5 Caexlaer civ-awr aqyawf-anr jaceuq-awq;
\q1 Caexlaer yawkhav-aer civ-awr jaceuq-awq!
\q1
\v 6 Saf ghawr-eu yawqjeq navluf, Ya’we Miqyaer-anr jaceuq-awq!
\q1 Haleluya!
